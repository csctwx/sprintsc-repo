				</xs:annotation>
				
<!--Format of SSN should be xxxxxxxxx.-->
			</xs:element>
			
<xs:element name="additionalOrderInfo" type="tns:AdditionalOrderInfoType" minOccurs="0">
				
<xs:annotation>
					
