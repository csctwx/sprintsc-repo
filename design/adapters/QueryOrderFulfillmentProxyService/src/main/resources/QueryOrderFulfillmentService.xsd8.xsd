<xs:minLength value="14"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="PaymentCardTypeCodeType">
		
<xs:annotation>
			
<xs:documentation>Payment Card Type</xs:documentation>
		</xs:annotation>
		
<xs:restriction base="xs:string">
			
<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="PaymentMethodCodeType">
		
