<xs:restriction base="tns:TaxAmountType">
						
<xs:totalDigits value="11"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
<xs:element name="totalAmount" type="tns:ChargeAmountType" minOccurs="0">
				
