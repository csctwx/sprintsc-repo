<xs:annotation>
					
<xs:documentation>Total quantity of equipment in the order.

quantity_req</xs:documentation>
				</xs:annotation>
				
<xs:simpleType>
					
<xs:restriction base="tns:OrderItemQuantityType">
						
<xs:totalDigits value="9"/>
					</xs:restriction>
				</xs:simpleType>
