<xs:complexType name="OrderItemInfoType">
		
<xs:sequence>
			
<xs:element name="orderLineItemNumber" type="tns:OrderLineItemNumberType">
				
<xs:annotation>
					
<xs:documentation>Line Sequence Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="itemId" type="tns:LineItemIdentifierType">
				
<xs:annotation>
					
<xs:documentation>Item Identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="orderItemStatusCd" type="tns:OrderStatusCodeType">
				
<xs:annotation>
					
<xs:documentation>Order Item Status Code.
Values for Regular Orders:
0  Future
1  Backorder
2  Allocated
3  Pick Ticket Printed
4  Pick Verified 
6  Shipped
C  Cancelled / Closed

Values for RMA Orders:
0  Authorized
1  Received</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="orderItemQuantity" type="tns:OrderItemQuantityType">
				
<xs:annotation>
					
<xs:documentation>Order Item Quantity holds the total number of units of the specified Product ID ordered in the ORDER ITEM instance.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="backOrderedQuantity" type="tns:BackOrderedQuantityType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Back Ordered Quantity holds the number of items on backorder.  Back Ordered Quantity should be equal to or less than Order Item Quantity.  If no items are backordered Back Ordered Quantity will be set to zero.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="orderItemCancelQuantity" type="tns:OrderItemCancelQuantityType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The quantity of items cancelled in the order.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="allocatedQuantity" type="tns:AllocatedQuantityType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Allocated Quantity represents the number of items (SKU) reserved for an order.  Only items in "Main" can be Reserved.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="totalShippedQuantity" type="tns:TotalShippedQuantityType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Total Shipped Quantity holds the number of items from this Order Item that has been shipped.  Total Shipped Quantity should be less than or equal to orderITemQuantity.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="orderItemReturnReasonCode" type="tns:OrderItemReturnReasonCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Order Item Return Reason Code communicates the reason why the Return Kit was requested for the specified Order Item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="orderItemStatusShortDesc" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Short Description for the status of the order line item in OMIM.</xs:documentation>
				</xs:annotation>
				
<xs:simpleType>
					
<xs:restriction base="tns:OrderProcessingStatusDescriptionType">
						
<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
<xs:element name="orderItemStatusDesc" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The description of the order line status in OMIM.</xs:documentation>
				</xs:annotation>
				
<xs:simpleType>
					
<xs:restriction base="tns:OrderProcessingStatusDescriptionType"/>
				</xs:simpleType>
			</xs:element>
			
<xs:element name="shipmentItemList" type="tns:ShipmentItemListType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Structure containing one to many shipmentItemInfo structures</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="OrderItemListType">
		
<xs:sequence>
			
<xs:element name="orderItemInfo" type="tns:OrderItemInfoType" maxOccurs="unbounded">
				
<xs:annotation>
					
<xs:documentation>Structure containing order line details.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="OrderKeyInfoType">
		
<xs:sequence>
			
<xs:element name="orderKey" type="tns:OrderKeyIdentifierType">
				
<xs:annotation>
					
