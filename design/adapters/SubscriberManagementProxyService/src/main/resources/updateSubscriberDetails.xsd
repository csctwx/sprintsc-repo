	<xs:element name="updateSubscriberDetails" type="tns:UpdateSubscriberDetailsRequestType"/>
	<xs:element name="updateSubscriberDetailsResponse" type="tns:UpdateSubscriberDetailsResponseType"/>
	<xs:complexType name="UpdateSubscriberDetailsRequestType">
		<xs:sequence>
			<xs:element name="info" type="tns:SearchCriteriaType">
				<xs:annotation>
					<xs:documentation>Contains valid BAN identifiers</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberApplicationProgrammingInterfaceInfo" type="tns:SubscriberApplicationProgrammingInterfaceInfoType">
				<xs:annotation>
					<xs:documentation>Contains all the subscriber information that may need to be updated</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="applicationDataInfo" type="tns:ApplicationDataInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains application data information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UpdateSubscriberDetailsResponseType">
		<xs:sequence>
			<xs:element name="message" type="tns:ConfirmMessageTextType">
				<xs:annotation>
					<xs:documentation>Message indicating transaction is completed successfully</xs:documentation>
				</xs:annotation>
				<!-- only for ReplyCompCode 4, 8 -->
			</xs:element>
			<xs:element name="warningMessageList" type="tns:WarningMessageListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>if a transaction is completed with warnings then this structure will hold the warnings, in addition this structure can also return informational messages.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ActivationPersonalIdentificationNumberInfoType">
		<xs:sequence>
			<xs:element name="activationPersonalIdentificationNumber" type="tns:PasswordPinType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Activation PIN number required to active Boost subscribers at BCGI. 

This field is optional.

The filed will be validate in the following conditions:
The field has a value.
The PIN_REQUIRED_IND column in the  ACCOUNT_TYPE is set to ‘Y’.
The subscriber status is reserve or active.

The field can be 14 character in length and alphanumeric.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionalLifelineInfoType">
		<xs:sequence>
			<xs:element name="activityInd" type="tns:BinaryExpressionType">
				<xs:annotation>
					<xs:documentation>Lifeline activity indicator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastActivityDate" type="tns:ActivityDateType">
				<xs:annotation>
					<xs:documentation>Last activity date for the Lifeline subscriber</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressInfoType">
		<xs:sequence>
			<xs:element name="applyAddress" type="tns:AddressVerificationSystemOverrideCodeEnsType">
				<xs:annotation>
					<xs:documentation>If this flag is set, accept the address even if it cannot be found in the address database.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="tns:AddressTypeCodeType">
				<xs:annotation>
					<xs:documentation>Type of Address (Military, Rural, PO, Street, Foreign)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="tns:StreetNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="line1" type="tns:PrimaryAddressLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Line 1 for address type M or F</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="line2" type="tns:SecondaryAddressLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Line 2 (may include the urbanization name for Puerto Rico addresses)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ruralRoute" type="tns:RuralRouteControlNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rural Route if address type is Rural</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ruralRouteBoxNumber" type="tns:RuralRouteBoxNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rural Box number if address type is Rural</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postofficeBoxNumber" type="tns:PostOfficeBoxNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>PO Box number if address type is P</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetName" type="tns:StreetNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Name or Military Line 1</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="direction" type="tns:StreetDirectionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Direction</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetType" type="tns:StreetSuffixType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of street (e.g. Cir, Blvd, Rd)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartmentNumber" type="tns:ApartmentNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Apartment Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartmentType" type="tns:ApartmentDesignatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Apartment type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attention" type="tns:AttentionTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Attention Line</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="city" type="tns:CityNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>City Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="state" type="tns:UspsStateAbbreviationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>State Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="tns:Iso3CharCountryCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Country Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode" type="tns:UspsPostalCdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Zip code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode4" type="tns:UspsPostalCodeExtensionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Plus 4 zip code</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApplicationDataInfoType">
		<xs:sequence>
			<xs:element name="associateId" type="tns:AssociateIdentifierRsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Passed from external application (Radio Shack, RMS+) to Sprint in the Ensemble Billing system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="storeId" type="tns:ExternalRetailStoreIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Passed from external application (Radio Shack, RMS+) to Sprint in the Ensemble Billing system</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BogxUpdateInfoType">
		<xs:sequence>
			<xs:element name="offerTransactionId" type="tns:TransactionIdentifierType">
				<xs:annotation>
					<xs:documentation>BOGX offer transaction ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="offerLineCode" type="tns:OfferIdentifierType">
				<xs:annotation>
					<xs:documentation>BOGX offer line code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="price" type="tns:PriceAmountType">
				<xs:annotation>
					<xs:documentation>BOGX price</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalDiscount" type="tns:DiscountAmountType">
				<xs:annotation>
					<xs:documentation>As defined in the EQUIPMENT_OFFER_PRICING table</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommitmentInfoType">
		<xs:sequence>
			<xs:element name="commitReasonCode" type="tns:CommitmentReasonCodeType">
				<xs:annotation>
					<xs:documentation>The commitment reason code. Valid values are:
EQU – equipment related commitment
Other values defined in the COMMIT_REASON table</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commitOriginationNumberMonth" type="tns:CommitmentPeriodMonthQuantityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Duration of the commitment period applied to the subscriber. Valid values are between 0 (included) and 99 (included)
Zero indicates that no commitment period will be applied</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commitStartDate" type="tns:CommitmentStartDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The start date of the commitment</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DirectConnectInfoType">
		<xs:sequence>
			<xs:element name="alias" type="tns:SubscriberAliasType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the subscriber (alias).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FrequencyPeriodInfo">
		<xs:sequence>
			<xs:element name="serialNumber" type="tns:DeviceSerialNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A serial number for associating the new UFP.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endDate" type="tns:EffectiveThruDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date where the period ends</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bogxUpdateInfo" type="tns:BogxUpdateInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>BOGX Information</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LifelineInfoType">
		<xs:sequence>
			<xs:element name="lifelineKey" type="tns:ExternalSystemTokenKeyIdentifierType">
				<xs:annotation>
					<xs:documentation>Lifeline key</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lifelineExpirationDate" type="tns:ExpirationDateType">
				<xs:annotation>
					<xs:documentation>Lifeline expiration date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lifelineInd" type="tns:BinaryExpressionType">
				<xs:annotation>
					<xs:documentation>Lifeline indicator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalLifelineInfo" type="tns:AdditionalLifelineInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Additional life line information</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LongDistanceCarrierInfoType">
		<xs:sequence>
			<xs:element name="longDistanceCarrierCode" type="tns:LongDistanceCarrierCodeType">
				<xs:annotation>
					<xs:documentation>Long distance carrier code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dealerAgent" type="tns:DealerIdType">
				<xs:annotation>
					<xs:documentation>IXC Dealer code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="memoUserText" type="tns:NoteTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation> The user free text memo</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MemoInfoType">
		<xs:sequence>
			<xs:element name="memoSource" type="tns:MemoSourceCodeType">
				<xs:annotation>
					<xs:documentation>The source of the memo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="userText" type="tns:MemoTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>User text</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NameInfoType">
		<xs:sequence>
			<xs:element name="nameFormat" type="tns:NameFormatCodeType">
				<xs:annotation>
					<xs:documentation>Holds the name format (D - Business, P - individual)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="first" type="tns:FirstNameEnsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>First Name of individual customer – in the case of business customers, this may not exist</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="middle" type="tns:MiddleNameEnsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Middle initial name of individual customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="last">
				<xs:annotation>
					<xs:documentation>Last name of individual user or Business Name of business customer</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:LastNameType">
						<xs:minLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="title" type="tns:TitleEnsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Title code of individual customer </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="generation" type="tns:GenerationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Generation of customer name (e.g. III)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalLine" type="tns:AdditionalTitleType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Additional line for BAN name</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NineOneOneConsentInfoType">
		<xs:sequence>
			<xs:element name="signed911ConsentFlag" type="tns:Consent911FlagType">
				<xs:annotation>
					<xs:documentation>Flag indicating if 911 consent is signed
Should be sent as true if 911 consent is signed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="textInfo" type="tns:ConsentStatementTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>User entered text </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OtherCommunicationMethodsType">
		<xs:sequence>
			<xs:element name="communicationEmail" type="tns:EmailAddressStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The value of the Email Communication Method</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="communicationFax" type="tns:PhoneNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The value of the Fax Communication Method</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaperWorkInfoType">
		<xs:sequence>
			<xs:element name="paperWorkStatus" type="tns:PaperWorkStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies the status of the paper work.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paperWorkDate" type="tns:PaperWorkCompletedDateEnsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Paper work date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paperWorkReferenceNumber" type="tns:PaperWorkReferenceNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Paper work reference number</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SearchCriteriaType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="subscriberId" type="tns:SubscriberIdentifierType">
					<xs:annotation>
						<xs:documentation>Subscriber number</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="mdn" type="tns:MobileDirectoryNumberStringType">
					<xs:annotation>
						<xs:documentation>Phone number of any subscriber under the BAN.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ufmi" type="tns:UrbanFleetMemberIdentifierStringType">
					<xs:annotation>
						<xs:documentation>Member ID (UrbanId*FleetId*MemberId) of any subscriber under the BAN</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="deviceSerialNumber" type="tns:DeviceSerialNumberType">
					<xs:annotation>
						<xs:documentation>Equipment ID – IMEI/Simcard ID/MSN/ESN of any subscriber under the BAN</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ipAddress" type="tns:InternetProtocolAddressStringType">
					<xs:annotation>
						<xs:documentation>IP address of any subscriber under the BAN</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imsi" type="tns:InternationalMobileSubscriberIdentityStringType">
					<xs:annotation>
						<xs:documentation>IMSI/MSID of any subscriber under the BAN </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="nai" type="tns:NetworkAccessIdentifierType">
					<xs:annotation>
						<xs:documentation>NAI address of the subscriber to be updated</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="additionalImsi" type="tns:InternationalMobileSubscriberIdentityStringType">
					<xs:annotation>
						<xs:documentation>Additional IMSI of the subscriber to be updated</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="hppttId" type="tns:HighPerformancePushToTalkIdStringType">
					<xs:annotation>
						<xs:documentation>The subscriber’s HPPTT ID. The ID will be constructed from username@domain, where the username will be a PTN or UFMI-based number.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="timeStampInfo" type="tns:TimestampInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure providing  BAN timestamp date and timestamp.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="custTimeStampInfo" type="tns:TimestampInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure providing customer timestamp date and timestamp</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberTimeStampInfo" type="tns:TimestampInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure providing subscriber timestamp date and timestamp</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
