	<!---->
	<xs:element name="submitOrder" type="tns:SubmitOrderType"/>
	<xs:element name="submitOrderResponse" type="tns:SubmitOrderResponseType"/>
	<!---->
	<xs:complexType name="SubmitOrderType">
		<xs:sequence>
			<xs:element name="callingApplicationInfo" type="tns:CallingApplicationInfoType">
				<xs:annotation>
					<xs:documentation>This structure defines the fields for the calling application</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderInfo" type="tns:OrderInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This structure defines the fields for the order</xs:documentation>
				</xs:annotation>
				<!-- This tag should be populated in activation requests only for Upgrade/Replace order types-->
				<!-- It should be populated when the ptn provided for an Upgrade/Replace order  in the credit check            
					phase is different from the ptn provided in the activation request although they belong to the same account number.
					If the ptn doesn't belong to the same account for which the credit check is done, then it will be errored back. 
					If account-number is passed here, it will be ignored. Only ptn will be accepted. -->
			</xs:element>
			<xs:element name="accountTypeInfo" type="tns:AccountTypeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure for account type information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="securityInfo" type="tns:SecurityInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure for security information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditCheckInfo" type="tns:CreditCheckInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure for credit check information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerInfo" type="tns:CustomerInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure for customer information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shippingInfo" type="tns:ShippingInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This structure defines the fields for the calling application</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentEnrollmentInfo" type="tns:PaymentEnrollmentInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>to replace rccpWanted and rccpPayment structures</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="devicePaymentInfo" type="tns:DevicePaymentInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This structure defines the fields for the device Payments </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="depositPaymentInfo" type="tns:PaymentCardInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This structure defines the fields for the deposits </xs:documentation>
				</xs:annotation>
				<!--Credit card payment is required for deposit payment.-->
			</xs:element>
			<xs:element name="equipmentEstimatedTax" type="tns:TaxAmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field is the estimated taxes for an equipment order.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="webToRetailOrderInfo" type="tns:WebToRetailOrderInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This structure defines the elements that will be called specifically for W2R orders.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tsrFraudInd" type="tns:FlaggedAsFraudFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates if TSR Fraud was detected on the order by the Telesales Agent.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderSubLineList" type="tns:OrderSubLineListType">
				<xs:annotation>
					<xs:documentation>This structure defines the fields (structure) for the Lines</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- GBL 12.3 VDS-->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubmitOrderResponseType">
		<xs:choice>
			<xs:element name="fastOrderKey" type="tns:FastOrderKeyIdentifierType">
				<xs:annotation>
					<xs:documentation>Fast Order Key is an order identifier provided by FAST Fulfillment and Activation Service Tool.  FAST is a browser-based order processing system designed to service, fulfill, and activate phone and accessory orders generated by various Nextel sales channels. The queue-based workflow allows users in different areas of the country to simultaneously process customer orders that are in different stages of the fulfillment and activation </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="activationResponseInfo" type="tns:DoActivationResponseType"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="AccessoryResponseType">
		<xs:sequence>
			<xs:element name="accessoryId" type="tns:SequenceNumberLongType">
				<xs:annotation>
					<xs:documentation>Echoes back PARTNER defined accessory order line number provided in Activation Request</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccessoryType">
		<xs:sequence>
			<xs:element name="accessoryId" type="tns:SequenceNumberType">
				<xs:annotation>
					<xs:documentation>Uniquely defines each distinct accessory “order line”</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="itemId" type="tns:ModelNumberType">
				<xs:annotation>
					<xs:documentation>Ensemble item Id (model sku)</xs:documentation>
				</xs:annotation>
				<!--The model-id for the accessory-->
			</xs:element>
			<xs:choice>
				<xs:element name="modelPrice" type="tns:ItemRetailPriceType">
					<xs:annotation>
						<xs:documentation>Price of the accessory.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="priceCode" type="tns:PriceCodeType">
					<xs:annotation>
						<xs:documentation>price code for the accessory.  code that is translated to actual price for accessory in backend.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="bogxOfferId" type="tns:OfferIdentifierType">
					<xs:annotation>
						<xs:documentation>BOGX offer ID</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="quantity" type="tns:InventoryItemQuantityType">
				<xs:annotation>
					<xs:documentation>number of items for this order line</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discretionaryCredit" type="tns:CreditAmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Optionally provided credit amount for the accessory being purchased.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bogxTransactionInfo" type="tns:BogxTransactionInfoType" minOccurs="0"/>
			<xs:element name="shipToCode" type="tns:ShipToTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code to determine the delivery mechanism for this order 
STS – Ship to Store
STH – Ship to Home</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rmaNumber" type="tns:ReturnMerchandiseAuthorizationNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If the activity is a return the return merchandise authorization number will be returned if available.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccountInfoType">
		<xs:choice>
			<xs:element name="ban" type="tns:BillingAccountNumberType">
				<xs:annotation>
					<xs:documentation>Account Number uniquely identifies an Account.  The Account may be a Financial Account, a Customer Account, Billing Account, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mdn" type="tns:MobileDirectoryNumberStringType">
				<xs:annotation>
					<xs:documentation>Mobile Directory Number (MDN) is the actual phone number one would dial to reach a specific mobile phone.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberId" type="tns:SubscriberIdentifierType">
				<xs:annotation>
					<xs:documentation>Subscriber Identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="AccountTypeInfoType">
		<xs:sequence>
			<xs:choice minOccurs="0">
				<xs:annotation>
					<xs:documentation>Establishes the Account Type/SubType of new BAN</xs:documentation>
				</xs:annotation>
				<xs:element name="customerType" type="tns:CustomerTypeCodeType">
					<xs:annotation>
						<xs:documentation>Describes the type of customer. 
If this field is not populated, then accountType and accountSubType will need to be populated.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="accountType" type="tns:BillingAccountTypeCodeType">
						<xs:annotation>
							<xs:documentation>Describes the account type.  If this field is populated, then customerType field will be blank and accountSubType must be populated.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="accountSubType" type="tns:BillingAccountSubTypeCodeType">
						<xs:annotation>
							<xs:documentation>Describes the account subtype  If this field is populated, then customerType field will be blank and the accountType field must be populated.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:choice>
			<xs:element name="language" type="tns:LanguagePreferenceCodeType" minOccurs="0">
				<xs:annotation>
