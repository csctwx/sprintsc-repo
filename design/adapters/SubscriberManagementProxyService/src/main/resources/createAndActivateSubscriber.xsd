	<xs:element name="createAndActivateSubscriber" type="tns:CreateAndActivateSubscriberType"/>
	<xs:element name="createAndActivateSubscriberResponse" type="tns:CreateAndActivateSubscriberResponseType"/>
	<xs:complexType name="CreateAndActivateSubscriberType">
		<xs:sequence>
			<xs:element name="ban" type="tns:BillingAccountNumberType">
				<xs:annotation>
					<xs:documentation>The unique identifier for the new billing account. (BAN)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="networkInd" type="tns:NetworkTypeCodeType" block="restriction">
				<xs:annotation>
					<xs:documentation>Identfies the subscribrs network. C for CDMA and I for iDEN</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="resourceInfo" type="tns:ResourceInfoType">
				<xs:annotation>
					<xs:documentation>Additional resources that may be required for the plan features or device.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addressInfo" type="tns:AddressInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing detailed address information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nameInfo" type="tns:NameInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This structure contains the contact name of the account.  Only add if differs from billing (won't hurt to add)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberDetailsInfo" type="tns:SubscriberDetailsInfoType">
				<xs:annotation>
					<xs:documentation>Contains information about subscriber details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="newPricePlanInfo" type="tns:NewPricePlanInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>New Price Plan Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="newSocList" type="tns:NewSocListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about the new  add-on SOCS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="changedSocInfoList" type="tns:ChangedSocListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies the SOCs existing in the BAN to be excluded from the subscriber</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="featureInfoList" type="tns:FeatureListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Features associated with this SOC.  Can only be provided when action = 'A'.  If features are not provided, we will choose default features.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractInfo" type="tns:ContractInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contract information.  Sent if the contract period needs changed with this request.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="callingApplicationInfo" type="tns:CallingApplicationInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure in which to insert calling application and user info.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deviceList" type="tns:DeviceListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of device information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dacId" type="tns:DepartmentAccountCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The departmental accounting code (aka parent id) tied to the customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ssitInfo" type="tns:SsitInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This sub-structure provides SSIT Label and Value information.  The SSIT Labels are set at the hierarchy root level.  The SSIT values are the subscriber-specific values that correspond to each Label.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CreateAndActivateSubscriberResponseType">
		<xs:sequence>
			<xs:element name="subscriberId" type="tns:SubscriberIdentifierType">
				<xs:annotation>
					<xs:documentation>The Subscriber Identifier is a string that uniquely identifies a subscriber.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mdn" type="tns:PhoneNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Mobile Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ufmi" type="tns:UrbanFleetMemberIdentifierStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Urban Fleet Member Identifier 
(UFMI) is the combinaiton of the Urban Area Id, Fleet Id, and Member Id representing an address in a format recognized within iDEN Push-To-Talk (PTT) or Direct Connect. For example, a UFMI number of 500•600•700, translates to 500 is the Urban Area ID, 600 is the Fleet, and 700 is the Member.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ipAddress" type="tns:InternetProtocolAddressStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Internet Protocol Address
An Internet Protocol Address is the numerical address by which a location in the Internet is identified. Computers on the Internet use IP addresses to route traffic and establish connections among themselves.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nai" type="tns:NetworkAccessIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Network Access Identifier
 (NAI) is a standard way of identifying users who request access to a network. 
The standard syntax is"user@realm".</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cdmaProgrammingInfo" type="tns:CdmaProgrammingInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Holds the manual programming resources – Will be returned for CDMA subscribers only.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hppttId" type="tns:HighPerformancePushToTalkIdentifierStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Resource that supports HP-PTT services. The HPPTT ID can either be PTN-based or UFMI based. Will be returned for HPPTT subscribers only.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="banPoolInd" type="tns:BinaryExpressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicate where the CDMA IP was allocated from.
true – From the BAN pool false – From the common pool
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="securityLockCode" type="tns:PhoneLockCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customers use the Security Lock Code to unlock their handset, for CDMA phones.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="activationErrorInfo" type="tns:ActivationErrorInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Deposit error information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="warningErrorList" type="tns:WarningErrorListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of warning error information structures</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ActivationErrorInfoType">
		<xs:sequence>
			<xs:element name="errorCode" type="tns:ErrorMessageIdentifierType">
				<xs:annotation>
					<xs:documentation>The error code received when an error occurred while activating the subscriber</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="errorMessage" type="tns:ErrorMessageType">
				<xs:annotation>
					<xs:documentation>The error message received when an error occurred while  activating the subscriber.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressInfoType">
		<xs:sequence minOccurs="0">
			<xs:element name="applyAddress" type="tns:AddressVerificationSystemOverrideCodeType">
				<xs:annotation>
					<xs:documentation>Indicates whether the address is accepted even though it cannot be found in the address database.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addressType" type="tns:AddressTypeCodeType">
				<xs:annotation>
					<xs:documentation>Type of Address (Military, Rural, PO, Street, Foreign)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetNumber" type="tns:StreetNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Address Street Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="line1" type="tns:PrimaryAddressLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The content of Primary Address Line is dependent upon the type of street.  
For a ‘street address’, Primary Address Line holds the concatenation of the street components of the address.
For a ‘Post Office Box address’ (PO Box address), Primary Address Line holds the P.O. Box information.
For a ‘Rural address’, Primary Address Line holds the Rural number.
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="line2" type="tns:SecondaryAddressLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Secondary Address Line holds free text describing additional address information. Usually used for business customers.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ruralRoute" type="tns:RuralRouteControlNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rural Route</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ruralRouteBoxNumber" type="tns:RuralRouteBoxNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rural Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="poBoxNumber" type="tns:PostOfficeBoxNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Post Of Office Box Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetName" type="tns:StreetNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Address Street Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetDirection" type="tns:PrecedingStreetDirectionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Direction is a USPS Postal Address component that holds the compass direction that appears before the Street Name in some cases.  
For example in, "North Main Street", "North" would reside in Preceding Direction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetType" type="tns:StreetSuffixType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Address Street Type is a suffix for USPS Postal Address component that holds the trailing part of the street name.  
For example in, "123 Main Street", "Street" would reside in Street Suffix.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartmentNumber" type="tns:ApartmentNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Apartment Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartmentType" type="tns:ApartmentDesignatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Apartment Type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attention" type="tns:AttentionTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Attention Text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="city" type="tns:CityNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>City Name holds the textual string identifying a City.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="state" type="tns:UspsStateAbbreviationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A United States Postal Service (USPS) State Abbreviation Code is the set of characters that are selected from a State or Protectorate name and specified by the USPS to represent the name of a State or US Protectorate for the purpose of mail delivery.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="tns:IsoCountryCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An International Standardization Organization (ISO) 3-Character Country Code is the set of three characters assigned by the ISO to identify a sovereign nation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode" type="tns:UspsPostalCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>USPS Postal Code contains the first 5 characters of the nine digits USPS Postal Code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode4" type="tns:UspsPostalCodeExtensionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>USPS Postal Code Extension contains the last 4 characters of the nine digits USPS Postal Code.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CallingApplicationInfoType">
		<xs:sequence>
			<xs:element name="associateId" type="tns:AssociateIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Associate identifier. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="storeId" type="tns:RetailStoreIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Store identifier.  </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CdmaProgrammingInfoType">
		<xs:sequence>
			<xs:element name="mdn" type="tns:PhoneNumberType">
				<xs:annotation>
					<xs:documentation>Mobile Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deviceSerialNumber" type="tns:DeviceSerialNumberType">
				<xs:annotation>
					<xs:documentation>MSN/HSN/MEID or other serial number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="msid" type="tns:MobileStationIdentificationNumberType">
				<xs:annotation>
					<xs:documentation>Mobile Station Identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sidBid" type="tns:SidNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SID – Approved 5-digit number that has been assigned to identify a particular cellular carrier. This number identifies your “home” system.
BID – Approved Identification number administered by CIBERNET for billing purposes only. Separately identifies a portion of a wireless service area used to geographically subdivide System Identification Numbers (SIDs).
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="oneTimeSubsidyLock" type="tns:OneTimeKeySubsidyLockType">
				<xs:annotation>
					<xs:documentation>One Time Subsidy Lock (OTKSL)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternativeLock" type="tns:MasterSubsidyLockCodeType">
				<xs:annotation>
					<xs:documentation>Master Subsidy Lock (MSL)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturerId" type="tns:ManufacturerPartyIdentifierType">
				<xs:annotation>
					<xs:documentation>Phone Manufacturer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modelId" type="tns:ModelNumberType">
				<xs:annotation>
					<xs:documentation>Device model number</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ChangedSocListType">
		<xs:sequence>
			<xs:element name="changedSocInfo" type="tns:ChangedSocInfoType" maxOccurs="200">
				<xs:annotation>
					<xs:documentation>Information about the price plan and add-on SOCS to be excluded</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ChangedSocInfoType">
		<xs:sequence>
			<xs:element name="soc" type="tns:ServiceOrderCodeType">
				<xs:annotation>
					<xs:documentation>SOC code for service to be excluded</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContractInfoType">
		<xs:sequence>
			<xs:element name="reason" type="tns:ContractChangeReasonCodeType">
				<xs:annotation>
					<xs:documentation>Reason of contract change. “EQU” – Equipment Update.
“PPD” - Standard (New PP/SOC) “EQP" – New equipment
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="durationInMonths" type="tns:NumberOfContractMonthsType">
				<xs:annotation>
					<xs:documentation>Duration of contract in total months from the start date.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeviceInfoType">
		<xs:sequence>
			<xs:element name="serialNumber" type="tns:DeviceSerialNumberType">
				<xs:annotation>
					<xs:documentation>Serial number of the device</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="serialType" type="tns:DeviceSerialNumberTypeCodeType">
				<xs:annotation>
					<xs:documentation>Serial type of the device</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeviceListType">
		<xs:sequence>
			<xs:element name="deviceInfo" type="tns:DeviceInfoType" maxOccurs="50">
				<xs:annotation>
					<xs:documentation>Information about the device</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeatureInfoType">
		<xs:sequence>
			<xs:element name="featureCode" type="tns:FeatureCodeType">
				<xs:annotation>
					<xs:documentation>Code for this feature.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expirationDate" type="tns:ExpirationDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date the feature is to expire (used only to exclude a feature).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paramDataInfo" type="tns:ParamDataInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Parameter data Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="m2hoRangeList" type="tns:M2HoRangeListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Structure provides details for a set of M2H/O Numbers/Ranges assigned to a M2H/O PP/SOC.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="staticIpInfo" type="tns:StaticIpInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CDMA Static IP resource feature.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeatureListType">
		<xs:sequence>
			<xs:element name="featureInfo" type="tns:FeatureInfoType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Information about the feature codes to be included in or excluded from the SOC</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HomeDomainRestrictionInfoType">
		<xs:sequence>
			<xs:element name="homeDomainRestrictionIpPairA" type="tns:InternetProtocolAddressPairType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Header for first pair of IPs</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="homeDomainRestrictionIpPairB" type="tns:InternetProtocolAddressPairType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Header for second pair of IPs</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="M2HoRangeListType">
		<xs:sequence>
			<xs:element name="rangeInfo" type="tns:RangeInfoType" maxOccurs="3">
				<xs:annotation>
					<xs:documentation>The Structure provides details for a set of M2H/O Numbers/Ranges assigned to a M2H/O PP/SOC.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MobileNodeRestrictionInfoType">
		<xs:sequence>
			<xs:element name="mobileNodeRestrictionIpA" type="tns:InternetProtocolAddressStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>First IP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mobileNodeRestrictionIpB" type="tns:InternetProtocolAddressStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Second IP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mobileNodeRestrictionIpC" type="tns:InternetProtocolAddressStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Third IP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mobileNodeRestrictionIpD" type="tns:InternetProtocolAddressStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Fourth IP</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NameInfoType">
		<xs:sequence>
