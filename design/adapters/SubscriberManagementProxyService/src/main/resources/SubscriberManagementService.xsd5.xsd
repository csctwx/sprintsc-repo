<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://integration.sprint.com/interfaces/manageSubscriberServices/v1/manageSubscriberServices.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.4" xmlns:tns="http://integration.sprint.com/interfaces/manageSubscriberServices/v1/manageSubscriberServices.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <!--Date		        Changed by	                Version		        Change description-->
  <!--03/04/2009	        Ibrahim ElKhatib	1.1	                        CS#311509: Changed the element in manageSubscriberServices from SocInfoList to socInfoList. Changed the element in socInfoList from SocInfo to socInfo-->
  <!--04/15/2009                      Ibrahim ElKhatib               1.2                                   CS#317263:Changed resourceType to an optional field. Removed the enumeration from NetworkResourceTypeCode.-->
  <!--04/17/2009                      Ibrahim ElKhatib               1.3                                   CS#317506:Changed resourceNumber to an optional field.-->
  <!--3/23/2010                      Tracy Turner               2.0	     Added optional field additionalSocInd to request	 GBL10.3              PJ000177-->
  <!--05/3/2010                      Tracy Turner               2.1	     CS#383599:  Added optional structure callingApplicationInfo to the request containg vendorCode and pin GBL10.3              PJ000177-->
  <!--05/03/2010          Steve Heichelheim  2.1              applied MVM standards and simple types                   GBL10.3              PJ000177-->
  <!--5/7/2010                     Tracy Turner             3.0	     Added optional field gvId to socInfo	 GBL10.4              PJ003652-->
  <!--6/7/2010                     Tracy Turner              3.1	     Moved optional field gvId to resourceInfo, changed type to 'gvIdentifierType', maxLength to 20	 GBL10.4              PJ003652-->
  <!--8/10/2010                    Tracy Turner             3.2	 Added partnerResourceList structure.  Changed resourceInfo.gvId to partnerResourceList.partnerResourceInfo.partnerResourceId																				GBL10.4              PJ003652-->
  <!--09/13/2010          		Suneel Pathipati  		3.3              added a new field skipServiceValidationInd in request   GBL10.4              PJ002221 CR#21-->
  <!--09/27/2010          		Tracy Turner  		4.0             added a new field preValidRequestInd in request, preValidatedInd in response   GBL10.5              PJ004332 CR#2-->
  <!--04/19/2012          		Dweepant Bakshi  		4.1             increased ength of ngp from 6 tpo 10  Prod Fix              IM962446 CS#526031-->
  <!--01/06/2012                       Ace Cineus                      4.1                                  Increased maxLen restriction of DeviceSerialNumberType from 18 to 20-->
  <!--07/20/2012              Steve Heichelheim    4.2    Made the sequence for pin & vendorCode in callingApplicationInfo optional and added another optional sequence and element for dealerCode     GBL12.3 PJ007444-->
  <!--08/08/2012              Ishita Biswas           4.3    Added optional fields associateId and storeId to the callingApplicationInfo structure     GBL12.3 PJ007444-->
  <!--08/13/2012              Ishita Biswas           4.4    Removed associateId from the callingApplicationInfo structure     GBL12.3 PJ007444-->
  <xs:element name="manageSubscriberServices" type="tns:ManageSubscriberServicesType"/>
  <xs:element name="manageSubscriberServicesResponse" type="tns:ManageSubscriberServicesResponseType"/>
  <xs:complexType name="ManageSubscriberServicesType">
    <xs:sequence>
      <xs:element name="subscriberInfo" type="tns:SubscriberInfoType"/>
      <xs:element name="newPlanInfo" type="tns:NewPlanInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>If changing the equipment requires a new plan, this will be provided.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="socInfoList" type="tns:SocInfoListType" minOccurs="0"/>
      <xs:element name="contractInfo" type="tns:ContractInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Contract information.  Sent if the contract period needs changed with this request.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="resourceInfo" type="tns:ResourceInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Additional resources that may be required for the plan features or device.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="govtCorporationId" type="tns:OrganizationIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Government or Corporation ID</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="featureChangeInfoList" type="tns:FeatureChangeInfoListType" minOccurs="0"/>
      <xs:element name="memoInfoList" type="tns:MemoInfoListType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A list of memos.  You can have up to two memos in this list, either a resource memo, a plan memo or both.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="prePaidPlanPolicyInfo" type="tns:PrepaidPolicyPlanInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Structure providing the Prepaid Plan Policy indicators</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="additionalActivityInfo" type="tns:AdditionalActivityInfoType" minOccurs="0"/>
      <xs:element name="barringNaiInfo" type="tns:BarrringNaiInfoType" minOccurs="0"/>
      <xs:choice minOccurs="0">
        <xs:element name="additionalSocInd" type="tns:BinaryExpressionType"/>
        <xs:element name="skipServiceValidationInd" type="tns:BinaryExpressionType"/>
      </xs:choice>
      <xs:element name="callingApplicationInfo" type="tns:CallingApplicationInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Structure in which to insert calling application and user info.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="partnerResourceList" type="tns:PartnerResourceListType" minOccurs="0"/>
      <xs:element name="preValidateRequestInd" type="tns:BinaryExpressionType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ManageSubscriberServicesResponseType">
    <xs:sequence>
      <xs:element name="resourceType" type="tns:NetworkResourceTypeCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The type of resource added. It can be cellular (C), dispatch (M), packet data (P) NAI (N), IMSI (I) or Hppt (H).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="resourceNumber" type="tns:ResourceDeviceSerialNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Resource Id</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="tempMdnInd" type="tns:BinaryExpressionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>true/false flag indicating whether or not a temporary PTN was allocated for the Port In (if resource type is C).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cdmaProgrammingInfo" type="tns:CdmaProgrammingInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Holds the manual programming resources – Will be returned for CDMA subscribers only.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="subscriberDetailsList" type="tns:SubscriberDetailsListType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>List of subscribers that have a mismatch add-on PP. Also returns an appropriate message for it.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hppttId" type="tns:HighPerformancePushToTalkIdentifierStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Resource that supports HP-PTT services. The HPPTT ID can either be PTN-based or UFMI based. Will be returned for HPPTT subscribers only.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="banPoolInd" type="tns:BinaryExpressionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Indicate where the CDMA IP was allocated from.
true – From the BAN pool false – From the common pool</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="preValidatedInd" type="tns:BinaryExpressionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>true/false flag indicating whether or not the request was successfully prevalidated.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalActivityInfoType">
    <xs:sequence>
      <xs:element name="createInteractionInd" type="tns:CreateInteractionIndType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Populated with a 'true' value by calling applications that create sView interactions.
All other calling applications can set the field to an 'false' value, leave it empty, or not send it at all.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BarrringNaiInfoType">
    <xs:sequence>
      <xs:element name="naiReason" type="tns:NetworkAccessIdentifierBarringReasonCodeType">
        <xs:annotation>
          <xs:documentation>The reason specifying whether to enable or disable NAI. The values would be:
Temporary Disable
Remove Temporary Disable</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CallingApplicationInfoType">
    <xs:sequence minOccurs="0">
      <xs:sequence minOccurs="0">
        <xs:element name="pin" type="tns:PasswordPinType">
          <xs:annotation>
            <xs:documentation>Pin Code</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="vendorCode" type="tns:VendorCodeType">
          <xs:annotation>
            <xs:documentation>Application specific identifier unique to the OVM system.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:element name="dealerCode" type="tns:DealerIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Dealer code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="storeId" type="tns:RetailStoreIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Passed from retail store front ends to the Ensemble Billing system. This is the ID of the store performing the transaction</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CdmaProgrammingInfoType">
    <xs:sequence>
      <xs:element name="mdn" type="tns:PhoneNumberType">
        <xs:annotation>
          <xs:documentation>Mobile Number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="deviceSerialNumber" type="tns:DeviceSerialNumberType">
        <xs:annotation>
          <xs:documentation>MSN/HSN/MEID or other serial number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="msid" type="tns:MobileStationIdentificationNumberType">
        <xs:annotation>
          <xs:documentation>Mobile Station Identifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sidBid" type="tns:SidNumberType">
        <xs:annotation>
          <xs:documentation>SID – Approved 5-digit number that has been assigned to identify a particular cellular carrier. This number identifies your “home” system.
BID – Approved Identification number administered by CIBERNET for billing purposes only. Separately identifies a portion of a wireless service area used to geographically subdivide System Identification Numbers (SIDs).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="oneTimeSubsidyLock" type="tns:OneTimeKeySubsidyLockType">
        <xs:annotation>
          <xs:documentation>One Time Subsidy Lock (OTKSL)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="alternativeLock" type="tns:MasterSubsidyLockCodeType">
        <xs:annotation>
          <xs:documentation>Master Subsidy Lock (MSL)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="manufacturerId" type="tns:ManufacturerPartyIdentifierType">
        <xs:annotation>
          <xs:documentation>Phone Manufacturer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="modelId" type="tns:ModelNumberType">
        <xs:annotation>
          <xs:documentation>Device model number</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ContractInfoType">
    <xs:sequence>
      <xs:element name="reason" type="tns:ContractChangeReasonCodeType">
        <xs:annotation>
          <xs:documentation>Reason of contract change. “EQU” – Equipment Update.
“PPD” - Standard.
“COR” - Correction Change Commitment.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="startDate" type="tns:ContractStartDateType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The date that the contract begins.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="contractMonths" type="tns:NumberOfContractMonthsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Duration of contract in total months from the start date.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="contractType" type="tns:ContractTypeCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Contract Type.
Valid Values :
1) ‘A’ – Upgrade.
2) ’B’ – Replacement.
3) ’C’ – New Equipment Purchase.
4) ’D’ – New Rate Plan.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FeatureChangeInfoType">
    <xs:sequence>
      <xs:element name="socSequenceNumber" type="tns:SequenceNumberType">
        <xs:annotation>
          <xs:documentation>Sequence number to uniquely identify the soc who's features are being changed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="featureCode" type="tns:FeatureCodeType">
        <xs:annotation>
          <xs:documentation>Identifies the feature on the soc to be changed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="paramInfo" type="tns:ParamInfoType" minOccurs="0"/>
      <xs:element name="mdnInfo" type="tns:MdnInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>MDN/PTN info if MSISDN is to be allocated (for circuit data feature)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="expirationDate" type="tns:ExpirationDateType" minOccurs="0"/>
      <xs:element name="npaNxxInfo" type="tns:NpaNxxInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>NPA-NXX code if MSISDN is to be allocated (for circuit data feature)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="m2hoInfo" type="tns:M2HoInfoType"/>
      <xs:element name="action" type="tns:UpdateParameterTypeCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The only  valid values are: UPDPARAM stands for Update Parameter
•	Empty tagThis element is only allowed to be specified by the schema in the FeatureApiInfo structure when the structure is used for existing features featureApiInfoList.
•	Sending UPDPARAM as action implies that the user is only updating the M2H/O Number(s)/ Range(s) without including or excluding the M2H/O feature.
•	Not sending this tag or sending an empty tag implies that the current action performed on the existing feature is include feature or exclude feature.
•	The system will ignore the action UPDPARAM if it is sent for a non-M2H/O feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="paramDataInfo" type="tns:ParamDataInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The structure contains the feature parameters for the generic parameters mechanism</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="staticIpInfo" type="tns:StaticIpInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>CDMA Static IP resource feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FeatureChangeInfoListType">
    <xs:sequence>
      <xs:element name="featureChangeInfo" type="tns:FeatureChangeInfoType" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FeatureInfoListType">
    <xs:sequence>
      <xs:element name="featureInfo" type="tns:FeatureInfoType" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FeatureInfoType">
    <xs:sequence>
      <xs:element name="featureCode" type="tns:FeatureCodeType">
        <xs:annotation>
          <xs:documentation>Code for this feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="expirationDate" type="tns:ExpirationDateType" minOccurs="0"/>
      <xs:element name="paramInfo" type="tns:ParamInfoType" minOccurs="0"/>
      <xs:element name="mdnInfo" type="tns:MdnInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>MDN/PTN info if MSISDN is to be allocated (for circuit data feature)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="npaNxxInfo" type="tns:NpaNxxInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>NPA-NXX code if MSISDN is to be allocated (for circuit data feature)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="m2hoRangeList" type="tns:M2HoRangeListType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Structure provides details for a set of M2H/O Numbers/Ranges assigned to a M2H/O PP/SOC.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="paramDataInfo" type="tns:ParamDataInfoType" minOccurs="0"/>
      <xs:element name="staticIpInfo" type="tns:StaticIpInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>CDMA Static IP resource feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="HomeDomainRestrictionInfoType">
    <xs:sequence>
      <xs:element name="homeDomainRestrictionIpPairA" type="tns:InternetProtocolAddressPairType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Header for first pair of IPs</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="homeDomainRestrictionIpPairB" type="tns:InternetProtocolAddressPairType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Header for second pair of IPs</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="IpAddressInfoType">
    <xs:sequence>
      <xs:element name="ipType" type="tns:InternetProtocolAddressTypeCodeType">
        <xs:annotation>
          <xs:documentation>IP address type required if the resource type is P.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ipPool" type="tns:InternetProtocolAddressPoolIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>IP pool code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="method" type="tns:AllocationMethodType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Method of allocation. It can be random (R).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="recoveryIp" type="tns:InternetProtocolAddressStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The IP number to recover. It is required only if the action is recovery (V) and the resource type is P.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="M2HoInfoType">
    <xs:sequence>
      <xs:element name="mdnEffectiveDate" type="tns:MobileDirectoryNumberAssignmentFromDateType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Mobile Directory Number Assignment From Date holds the first calendar day the Mobile Directory Number was assigned to the specified wireless device</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="m2hoRangeList" type="tns:M2HoRangeListType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Structure provides details for a set of M2H/O Numbers/Ranges assigned to a M2H/O PP/SOC.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="M2HoRangeListType">
    <xs:sequence>
      <xs:element name="rangeInfo" type="tns:RangeInfoType" maxOccurs="3"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MdnInfoType">
    <xs:sequence>
      <xs:element name="numberLocation" type="tns:NumberLocationCodeType">
        <xs:annotation>
          <xs:documentation>The number location.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="method" type="tns:AllocationMethodType">
        <xs:annotation>
          <xs:documentation>Method of allocation.  It can be random (R).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MemoInfoListType">
    <xs:sequence>
      <xs:element name="memoInfo" type="tns:MemoInfoType" maxOccurs="2">
        <xs:annotation>
          <xs:documentation>New Memo</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MemoInfoType">
    <xs:sequence>
      <xs:element name="type" type="tns:MemoTypeCodeType">
        <xs:annotation>
          <xs:documentation>The Memo Type.
P = Plan Memo
R = Resource Memo</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="source" type="tns:MemoSourceCodeType">
        <xs:annotation>
          <xs:documentation>A single character annotating the source of the memo.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="memoText" type="tns:MemoTextType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Memo Text is the body of textual content of an Ensemble MEMO.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MobileNodeRestrictionInfoType">
    <xs:sequence>
      <xs:element name="mobileNodeRestrictionIpA" type="tns:InternetProtocolAddressStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>First IP</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mobileNodeRestrictionIpB" type="tns:InternetProtocolAddressStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Second IP</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mobileNodeRestrictionIpC" type="tns:InternetProtocolAddressStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Third IP</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mobileNodeRestrictionIpD" type="tns:InternetProtocolAddressStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Fourth IP</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="NaiInfoType">
    <xs:choice>
      <xs:element name="userName" type="tns:NaiUserNameType">
        <xs:annotation>
          <xs:documentation>The username part of the NAI - a string with length of 3-20 characters.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="naiInd" type="tns:BinaryExpressionType">
        <xs:annotation>
          <xs:documentation>Indicates that the user wants to allocate a NAI.  If true, a NAI will be allocated.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="NewPlanInfoType">
    <xs:sequence>
      <xs:element name="pricePlanCode" type="tns:PricePlanCodeType">
        <xs:annotation>
          <xs:documentation>SOC code for new Plan</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="effectiveDate" type="tns:EffectiveFromDateType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Effective date of new plan.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ltsRank" type="tns:RankSequenceNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Lines To Share Rank.  Used if the new plan is a LTS plan.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="featureInfoList" type="tns:FeatureInfoListType" minOccurs="0"/>
      <xs:element name="waivedReason" type="tns:AdjustmentReasonCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>OTC Waived Off reason code. The value is retrieved from the reason_code field in the ADJUSTMENT_REASON reference table, where adj_actv_code = ‘FEW’ and man_adj_ind = ‘Y’</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orderKeyInfo" type="tns:OrderKeyInfoType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="NpaNxxInfoType">
    <xs:sequence>
      <xs:element name="npa" type="tns:NpaNumberType"/>
      <xs:element name="nxx" type="tns:NxxNumberType"/>
    </xs:sequence>
  </xs:complexType>
