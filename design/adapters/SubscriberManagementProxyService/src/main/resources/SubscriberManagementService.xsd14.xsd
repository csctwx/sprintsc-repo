<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://integration.sprint.com/interfaces/submitEquipmentPurchaseOrder/v1/submitEquipmentPurchaseOrder.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.5" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://integration.sprint.com/interfaces/submitEquipmentPurchaseOrder/v1/submitEquipmentPurchaseOrder.xsd">
  <!--Date              Author                   Version    Change                                                                                                 Rel                   SR#-->
  <!--08/06/2012  Steve Heichelheim    1.0      Initial version.  Added MVM standards and simple types.                      GBL12.3 PJ007442-->
  <!--10/29/2012  Subha Kannan    1.1      Added new structure lifeLineInfo in orderInfo  to support GBL13.1 PJ007219-->
  <!--06/02/2014  Nayanika Dey     1.2      Added new element "shippingTypeCode" in shippingInfo to fix Prod issue IM2394400 (Incorrect shipping type/vendor used for VMU Web orders).-->
  <!--Updated the paymentCardHolderName min length from 5 to 2.-->
  <!--06/03/2014 Adon Powell        1.2  standardize  add shipmentmethodidentifiertype, update cardholdername length              IM2394400 MAINT-->
  <!--06/10/2014 Adon Powell        1.2  change PaymentCardHolderNameType min length back to 5             IM2394400 MAINT-->
  <!--10/27/2014 Suresh Gundu     1.3  Added eppPaymentInfo structure, eppPaymentId and informationalRuleFailureReason element to authorizationInfo structure and taxTransactionInfo structure to the equipmentOrderLineInfo structure in the Request.		GBL 15.1	PJ012171-->
  <!--11/13/2014 Adon Powell        1.3  standardize GBL 15.1 PJ012171-->
  <!--11/17/2014 Suresh Gundu     1.4  Made paymentCardNumber and paymentCardSecurityCode optional, renamed informationalRuleFailureReason structure to businessRuleFailureList and businessRuleFailures strutcure to businessRuleFailureInfo.		GBL 15.1	PJ012171-->
  <!--11/18/2014 Suresh Gundu     1.5  Made the paymentCardNumber and paymentCardSecurityCode elements optional.		GBL 15.1	PJ012171-->
  <!--12/04/2014 Suresh Gundu     1.5  Added optional elements externalOrderId and globalTokenId to the eppPaymentInfo strutcure.		GBL 15.1	PJ012171-->
  <!--12/04/2014 Adon Powell        1.5  standardize GBL 15.1 PJ012171-->
  <!---->
  <xs:element name="submitEquipmentPurchaseOrder" type="tns:SubmitEquipmentPurchaseOrderRequestType"/>
  <xs:element name="submitEquipmentPurchaseOrderResponse" type="tns:SubmitEquipmentPurchaseOrderResponseType"/>
  <!---->
  <xs:complexType name="SubmitEquipmentPurchaseOrderRequestType">
    <xs:sequence>
      <xs:element name="callingApplicationInfo" type="tns:CallingApplicationInfoType">
        <xs:annotation>
          <xs:documentation>Structure provide calling application informatiom</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orderInfo" type="tns:OrderInfoType">
        <xs:annotation>
          <xs:documentation>Structure contains Order information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="shippingInfo" type="tns:ShippingInfoType">
        <xs:annotation>
          <xs:documentation>Customer Shipping information including name, address, phone numbers, email address, etc</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="billinginfo" type="tns:BillingInfoType">
        <xs:annotation>
          <xs:documentation>Customer billing information including name, address, phone numbers, email address, etc</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="devicePaymentInfo" type="tns:DevicePaymentInfoType">
        <xs:annotation>
          <xs:documentation>This structure defines the fields for the device Payments</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--Email Address for eBill option.-->
      <xs:element name="orderLineInfo" type="tns:OrderLineInfoType">
        <xs:annotation>
          <xs:documentation>Structure containing orderLines. each orderline can be device or accessory</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <!--Activation request message.-->
  </xs:complexType>
  <xs:complexType name="SubmitEquipmentPurchaseOrderResponseType">
    <xs:sequence>
      <xs:element name="fastOrderkey" type="tns:FastOrderKeyIdentifierType">
        <xs:annotation>
          <xs:documentation>Fast order key – returned for an asynchronous response</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!---->
  <xs:complexType name="AccountInfoType">
    <xs:choice>
      <xs:element name="ban" type="tns:BillingAccountNumberType">
        <xs:annotation>
          <xs:documentation>Account Number uniquely identifies an Account.  The Account may be a Financial Account, a Customer Account, Billing Account, etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mdn" type="tns:MobileDirectoryNumberStringType">
        <xs:annotation>
          <xs:documentation>Mobile Directory Number (MDN) is the actual phone number one would dial to reach a specific mobile phone.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="subscriberId" type="tns:SubscriberIdentifierType">
        <xs:annotation>
          <xs:documentation>Subscriber Identifier</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="AccountTypeInfoType">
    <xs:sequence>
      <xs:element name="accountType" type="tns:BillingAccountTypeCodeType">
        <xs:annotation>
          <xs:documentation>Describes the account type.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accountSubType" type="tns:BillingAccountSubTypeCodeType">
        <xs:annotation>
          <xs:documentation>Describes the account subtype.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressInfoType">
    <xs:sequence>
      <xs:element name="streetAddress1" type="tns:PrimaryAddressLineType">
        <xs:annotation>
          <xs:documentation>Street Address1</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="streetAddress2" type="tns:SecondaryAddressLineType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Street Address 2</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="city" type="tns:CityNameType">
        <xs:annotation>
          <xs:documentation>City</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="state" type="tns:UspsStateAbbreviationType">
        <xs:annotation>
          <xs:documentation>State</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zipCode" type="tns:UspsPostalCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>zipCode</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AuthorizationInfoType">
    <xs:sequence>
      <xs:element name="authorizationCode" type="tns:PaymentCardAuthorizationCodeType">
        <xs:annotation>
          <xs:documentation>Provide the successfull payment authorization code for device purchase</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="authorizationResponseDate" type="tns:AuthorizationDateType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Provide the authorizarion approval date,</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="eppPaymentId" type="tns:ExternalSystemReferenceNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Payment ID returned by EPP during an Authorization.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="businessRuleFailureList" type="tns:BusinessRuleFailureList" minOccurs="0">
        <xs:annotation>
          <xs:documentation>business rule failure List</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BillingInfoType">
    <xs:sequence>
      <xs:element name="nameInfo" type="tns:NameInfoType">
        <xs:annotation>
          <xs:documentation>Structure containing Customer Name inforamtion</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="emailAddress" type="tns:EmailAddressStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Customers email Address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="phoneNumberInfo" type="tns:PhoneNumberInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Structure containing Phone Number informatiom</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="addressInfo" type="tns:AddressInfoType">
          <xs:annotation>
            <xs:documentation>Structure Containing Customer Address information</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="cloneShippingAddressInd" type="tns:BinaryExpressionType">
          <xs:annotation>
            <xs:documentation>Indicate that billing Address is the same as shipping address</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CallingApplicationInfoType">
    <xs:sequence>
      <xs:element name="vendorCode" type="tns:VendorCodeType">
        <xs:annotation>
          <xs:documentation>Combination of vendorCode and pin is used to differentiate the calling system (channel).</xs:documentation>
        </xs:annotation>
        <!--Vendor code is a 2 letter code.-->
      </xs:element>
      <xs:element name="orderId" type="tns:PurchaseOrderNumberType">
        <xs:annotation>
          <xs:documentation>Order Identifier uniquely identifies a Equipmemt Purchase Order.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="salesChannel" type="tns:SalesChannelIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Channel identifier. Indicates which sales channel the order originated from</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="storeId" type="tns:RetailStoreIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Retail Store Identifer is the store ID passed from the retailer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="associateId" type="tns:AssociateIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Associate Identifier is the Retail Store Associate Identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DevicePaymentInfoType">
    <xs:sequence>
      <xs:element name="paymentType" type="tns:PaymentMethodTypeCodeType">
        <xs:annotation>
          <xs:documentation>Payment  Type  for which the customer wishes to pay for the equipment purchase.
CC
PP
RPV
AB</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="paymentAccountInfo" type="tns:PaymentAccountInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Structure Containing Payment Card Inforamtion or Paypal Info</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="authorizationInfo" type="tns:AuthorizationInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Structure contains payment authorization info</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="registeredPaymentInfo" type="tns:RegisteredPaymentInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Structure containing Registered Payment Vehicle information for VMU</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="eppPaymentInfo" type="tns:EppPaymentInfoType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EquipmentOrderLineInfoType">
    <xs:sequence>
      <xs:element name="orderLineId" type="tns:OrderLineItemNumberType">
        <xs:annotation>
          <xs:documentation>Uniquely identifies each distinct order line. The order Line Id should start with value ‘1’</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="modelId" type="tns:ModelNumberType">
        <xs:annotation>
          <xs:documentation>Ensemble item id (model sku), required for equipment purchase order lines</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="modelPrice" type="tns:PriceAmountType">
        <xs:annotation>
          <xs:documentation>Required for equipment purchase order lines.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="equipmentTaxAmt" type="tns:TaxAmountType">
        <xs:annotation>
          <xs:documentation>Required tax amount at order line</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accessoryInd" type="tns:BinaryExpressionType">
        <xs:annotation>
          <xs:documentation>Indicate the orderLine is device or accessory.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="taxTransactionInfo" type="tns:TaxTransactionInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>tax transaction info structure</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EquipmentOrderLineListType">
    <xs:sequence>
      <xs:element name="equipmentOrderLineInfo" type="tns:EquipmentOrderLineInfoType" maxOccurs="50">
        <xs:annotation>
          <xs:documentation>Equipment Order Line Structure</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="NameInfoType">
    <xs:sequence>
      <xs:element name="firstName" type="tns:FirstNameType">
        <xs:annotation>
          <xs:documentation>First Name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="middleInitial" type="tns:MiddleInitialType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Middle Initial</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lastName" type="tns:LastNameType">
        <xs:annotation>
          <xs:documentation>Last Name</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OrderInfoType">
    <xs:sequence>
      <xs:element name="orderTypeCode" type="tns:OrderTypeCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Order Type Code identifies what  type of order. The  default value will be EQP</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orderDate" type="tns:OrderEntryDateTimeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Order Entry Date holds the date the Order was entered into the system.</xs:documentation>
        </xs:annotation>
        <!--The date/time that the order was created by the customer.  
					Format is "CCYY-MM-DDThh:mm:ss"  (T is the date/time separator).-->
      </xs:element>
      <xs:element name="dealerCode" type="tns:DealerIdentifierType">
        <xs:annotation>
          <xs:documentation>The agent code identifies specific area specific “dealer” both external or internal that is responsible and should get credit for the associated transaction.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accountInfo" type="tns:AccountInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Account Information for existing customer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="accountTypeInfo" type="tns:AccountTypeInfoType">
        <xs:annotation>
          <xs:documentation>Structure containg account  type and sub type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="preOrderInd" type="tns:BinaryExpressionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>This represent  if the order is preOrder.  Pre-order equipment  and non-pre order equipment cannot be combined in a single order``</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lifeLineInfo" type="tns:LifeLineInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Structure containing Assurance Wireless Customers lifeline pin and the handset choice</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OrderLineInfoType">
    <xs:sequence>
      <xs:element name="orderLineList" type="tns:EquipmentOrderLineListType">
        <xs:annotation>
          <xs:documentation>List of Equipment Order Lines</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orderSubTotalAmount" type="tns:TotalAmountType">
        <xs:annotation>
          <xs:documentation>Order Sub Total Amount includes atotal equipment price</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orderTaxAmount" type="tns:TotalAmountType">
        <xs:annotation>
          <xs:documentation>Tax amount on the order</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orderTotalAmount" type="tns:TotalAmountType">
        <xs:annotation>
          <xs:documentation>Includes total equipment price and shipping price and tax amount</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PaymentAccountInfoType">
    <xs:choice>
      <xs:element name="paymentCardInfo" type="tns:PaymentCardInfoType">
        <xs:annotation>
          <xs:documentation>Structure Containing Payment Card Inforamtion</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="paypalInfo" type="tns:PayPalInfoType">
        <xs:annotation>
          <xs:documentation>Structure containing PayPal Information</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="PaymentCardInfoType">
    <xs:sequence>
      <xs:element name="paymentCardType" type="tns:PaymentCardTypeCodeType">
        <xs:annotation>
          <xs:documentation>Payment Card Type Code identifies the Credit/Debit Card provider.
Values:  V - Visa, M - MasterCard, A - Amex, D - Discover, DC - Diners Club</xs:documentation>
        </xs:annotation>
        <!--Credit card type.-->
      </xs:element>
      <xs:element name="paymentCardHolderName">
        <xs:annotation>
          <xs:documentation>Contains name of the Card Holder</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:PaymentCardHolderNameType"/>
        </xs:simpleType>
      </xs:element>
      <xs:element name="paymentCardNumber" type="tns:PaymentCardNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Payment Card Number is the unique identifier assigned to the Payment Card by the issuing financial institution.</xs:documentation>
        </xs:annotation>
        <!--Credit card number is no longer encrypted when sent in a request.-->
      </xs:element>
      <xs:element name="paymentCardSecurityCode" type="tns:PaymentCardSecurityCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Payment Card Security Code holds the 3 - 5 digit code located on the back of the Credit/Debit Card.</xs:documentation>
        </xs:annotation>
        <!--Security code is no longer encrypted.-->
      </xs:element>
      <xs:element name="paymentCardExpirationDate" type="tns:PaymentCardExpirationDateType">
        <xs:annotation>
          <xs:documentation>Payment Card Expiration Date is last calendar date the Credit/Debit Card is valid.</xs:documentation>
        </xs:annotation>
        <!--Format for expiration date is "CCYY-MM"-->
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PayPalInfoType">
    <xs:sequence>
      <xs:element name="paypalAccountName" type="tns:AccountNameConcatenationStringType">
        <xs:annotation>
          <xs:documentation>Provide the  paypal account name of the csutomer.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PhoneNumberInfoType">
    <xs:sequence>
      <xs:element name="homeNumber" type="tns:PhoneNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The customer’s home phone number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="workNumber" type="tns:WorkPhoneNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The customer’s work phone number</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="RegisteredPaymentInfoType">
    <xs:sequence>
      <xs:element name="rpvId" type="tns:RegisteredPaymentVehicleIdentifierType">
        <xs:annotation>
          <xs:documentation>Provide the information on registered payment vehicle id.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="rpvPaymentType" type="tns:PaymentMethodDescriptionType">
        <xs:annotation>
          <xs:documentation>Provide the payment type  CC, PP</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ShippingInfoType">
    <xs:sequence>
      <xs:element name="nameInfo" type="tns:NameInfoType">
        <xs:annotation>
          <xs:documentation>Structure containing the name information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="addressInfo" type="tns:AddressInfoType">
        <xs:annotation>
          <xs:documentation>Structure containing the address information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="emailAddress" type="tns:EmailAddressStringType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>email Address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="phoneNumberInfo" type="tns:PhoneNumberInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Phone Number Inforamation</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="shippingMethod" type="tns:ShipmentMethodCodeType">
        <xs:annotation>
          <xs:documentation>Shipping Method Description</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="shippingTypeCode" type="tns:ShipmentMethodIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Shipping Type is numeric code that uniquely determines the shipping method used.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="shippingFee" type="tns:ChargeAmountType">
        <xs:annotation>
          <xs:documentation>Shipping Fee</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LifeLineInfoType">
    <xs:sequence>
      <xs:element name="lifeLinePin" type="tns:LifeLinePinType">
        <xs:annotation>
          <xs:documentation>Assurance Wirless Customer LifeLine Pin provide by Solix.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="handsetChoice" type="tns:HandsetChoiceType" default="P">
        <xs:annotation>
          <xs:documentation>Handset Choice . Defaulted to 'P' for preimum handset</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!---->
  <!--MVM/Shared Simple Types-->
  <xs:simpleType name="AccountNameConcatenationStringType">
    <xs:annotation>
      <xs:documentation>Account Name Concatenation String is the concatenation of FIRST_NAME and LAST_BUSINESS_NAME from Billing Account database, with trailing spaces removed.  If the first name is blank or contains spaces, these will also be trimmed.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="5"/>
      <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AssociateIdentifierType">
    <xs:annotation>
      <xs:documentation>Associate Identifier is the Retail Store Associate Identifier.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="30"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AuthorizationDateType">
    <xs:annotation>
      <xs:documentation>Authorization Date represents a calendar Month, Day and Year on which the authorization took place.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date"/>
  </xs:simpleType>
  <xs:simpleType name="BillingAccountNumberType">
    <xs:annotation>
      <xs:documentation>Billing Account Number uniquely identifies an instance of Billing Account used to accrue charges for invoicing.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="10"/>
      <xs:minLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BillingAccountSubTypeCodeType">
    <xs:annotation>
      <xs:documentation>Billing Account SubType Code further categorizes the Billing Account within the context of the assigned Billing Account Type Code used primarily for Customer segmentation in General Ledger reporting and collection treatment.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BillingAccountTypeCodeType">
    <xs:annotation>
      <xs:documentation>Billing Account Type Code identifies the account type of the BILLING ACCOUNT. It is used mainly for segregation of the G/L reporting and Collection treatment.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BinaryExpressionType">
    <xs:annotation>
      <xs:documentation>Binary Expression is a type of an expression with two possible states,"true"and"false".</xs:documentation>
