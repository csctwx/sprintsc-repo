<xs:enumeration value="EnterpriseCustomer"/>
			
<xs:enumeration value="TPA"/>
			
<xs:enumeration value="TelecomMgrAdmin"/>
			
<xs:enumeration value="TelecomMgr"/>
			
<xs:enumeration value="CustomerCareAdvocate"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="RealmType">
		
