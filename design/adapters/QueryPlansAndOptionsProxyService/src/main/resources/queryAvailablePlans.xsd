	<!---->
	<xs:element name="queryAvailablePlans" type="tns:QueryAvailablePlansType"/>
	<xs:element name="queryAvailablePlansResponse" type="tns:QueryAvailablePlansResponseType"/>
	<!---->
	<xs:complexType name="QueryAvailablePlansType">
		<xs:sequence>
			<xs:element name="callingApplicationInfo" type="tns:CallingApplicationInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure in which to insert calling application and user info. This structure is being deprecated since it is being added as a common optional structure in WSMessageHeaderV2. New users of this API will use callingApplicationInfo of WSMessageHeaderV2 to populate the OVM header data. Existing may continue to use callingApplicationInfo of the body. The API will return an error if neither is populated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="searchCriteria" type="tns:SearchCriteriaType">
					<xs:annotation>
						<xs:documentation>Structure in which to identify criteria for plan search</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="searchBySocList" type="tns:SearchBySocListType">
					<xs:annotation>
						<xs:documentation>Contains the list of1 to 10  search by soc information structures.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QueryAvailablePlansResponseType">
		<xs:sequence>
			<xs:element name="pricePlanList" type="tns:PricePlanListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of price plan records.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="promoCodeInfo" type="tns:PromoCodeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing information relating to the promo code submitted in the request.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AffinityAttributeInfoType">
		<xs:sequence>
			<xs:element name="name" type="tns:AffinityAttributeNameType">
				<xs:annotation>
					<xs:documentation>Affinity Attribute name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="code" type="tns:AttributeKeyType">
				<xs:annotation>
					<xs:documentation>Affinity Attribute code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="tns:AffinityAttributeTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>C = Character, one char only.
N = Integer.
T = Date, with format MM/dd/yyyy
D = Date and Time with format MM/dd/yyyy HH:mm:ss
M = Decimal, meaning has exactly 2 digits after the decimal digit (3.25, 1.99, 19999.33 etc.)
S = String.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="length" type="tns:AffinityAttributeSizeQuantityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Length of affinity attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="requiredInd" type="tns:AffinityAttributeRequiredFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valid values are: true(Required) and false (Optional)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mask" type="tns:AffinityAttributeMaskType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The mask of the affinity attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pattern" type="tns:AffinityAttributePatternTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valid values are:  Number = #### Decimal(2) = ##,###.##
String = !!!
String = ^^^^ 
String = ###
String = aaaa
String = XXXXXX
Char = All String formats
Date = mm/dd/yyyy
Datetime = mm/dd/yyyy HH:mm:ss </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="value" type="tns:AffinityAttributeValueTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the default value for the attribute that may or may not be editable based on the editable tag.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editableInd" type="tns:AffinityAttributeEditableFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valid values are: true (Editable) and false (Not Editable)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AffinityAttributeListType">
		<xs:sequence>
			<xs:element name="affinityAttribute" type="tns:AffinityAttributeInfoType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Provides detail affinity info.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AffinityInfoType">
		<xs:sequence>
			<xs:element name="affinityRegistrationSoc">
				<xs:annotation>
					<xs:documentation>If associated price plan is an affinity benefit type then this value is the affinity registration feature that must also be selected</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:AffinityRegistrationServiceOrderCodeType"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="affinityName" type="tns:AffinityNameType">
				<xs:annotation>
					<xs:documentation>Name of the affinity </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="affinityAttributeList" type="tns:AffinityAttributeListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Affinity Attribute List</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AllowanceInfoType">
		<xs:sequence>
			<xs:element name="allowanceType" type="tns:AllowanceTypeCodeType">
				<xs:annotation>
					<xs:documentation>Type of allowance
Sprint 3G Data
Sprint 3G Data Roaming
Sprint 4G Data
Sprint 3G/4G Data</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="allowance" type="tns:UnitOfMeasureValueType">
				<xs:annotation>
					<xs:documentation>Included units of allowance for the input subscriber in MB</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AllowanceListType">
		<xs:sequence>
			<xs:element name="allowanceInfo" type="tns:AllowanceInfoType" maxOccurs="10"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BanInfoType">
		<xs:sequence>
			<xs:element name="ban" type="tns:BillingAccountNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing account number.

Required for an existing subscriber if mdn is not provided.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mdn" type="tns:MobileDirectoryNumberStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Subscriber phone number.

Required for an existing subscriber if ban is not provided.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:sequence>
					<xs:element name="accountType" type="tns:BillingAccountTypeCodeType">
						<xs:annotation>
							<xs:documentation>Account type.
Required if accountSubType is provided.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="accountSubType" type="tns:BillingAccountSubTypeCodeType">
						<xs:annotation>
							<xs:documentation>Account subscriber type.
Required if accountType is provided.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:element name="customerTypeDesc" type="tns:CustomerTypeCodeType">
					<xs:annotation>
						<xs:documentation>Required if account type and account  sub type is not provided.
INDIVIDUAL
BUSINESS
CORPORATE_INDIVIDUAL
CORPORATE_BUSINESS
GOVERNMENT_INDIVIDUAL
GOVERNMENT_BUSINESS</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="creditClass" type="tns:CreditClassCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Credit class of customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="corpId" type="tns:CorporationIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Corporate or government id.  Should be provide if customer type is corporate or government.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="numberSubscribers" type="tns:NumberOfSubscriptionsType">
				<xs:annotation>
					<xs:documentation>Number of subscribers on the order.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CallingApplicationInfoType">
		<xs:sequence>
			<xs:element name="pin" type="tns:PersonalIdentificationNumberType">
				<xs:annotation>
					<xs:documentation>Pin Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vendorCode" type="tns:VendorCodeType">
				<xs:annotation>
					<xs:documentation>Application specific identifier unique to the OVM system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderId" type="tns:ActivationOrderIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Order Id provided for Credit/Activation or Validation/Activation transactions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="storeId" type="tns:RetailStoreIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Store identifier.  Should be provided for Activation transactions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="associateId" type="tns:AssociateIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Associate identifier.  Should be provided for Activation transactions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="brandCode" type="tns:BrandCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>S - Sprint
N - Nextel
Defaulted to 'S' if not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DetailPlanInfoType">
		<xs:sequence>
			<xs:element name="totalMinutes" type="tns:MinutesIncludedQuantityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the total number of minutes the associated price plan includes </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="anytimeMinutes" type="tns:MinutesIncludedQuantityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the total number of anytime minutes the associated price plan includes </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nightWeekendMinutes" type="tns:MinutesIncludedTextType">
				<xs:annotation>
					<xs:documentation>Indicates the total number of night and weekend minutes the associated price plan includes </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extraAnytimeMinutesCharge" type="tns:OtherChargeAmountType">
				<xs:annotation>
					<xs:documentation>The per-minute overage charge that is assessed when anytime minutes have been exhausted in a single billing period</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="longDistanceCharge" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>The per-minute long distance charge.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitSmsCharge" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>Charge for SMS messaging if not included in plan or optional feature</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="callForwardingChargeRate" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>Charge to setup call forwarding with cell phone number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="etf" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>The early termination fee associated with this price plan</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="m2mMinutes" type="tns:MinutesIncludedQuantityType">
				<xs:annotation>
					<xs:documentation>Included mobile-to-mobile minutes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="intlChargePerMinute" type="tns:OtherChargeAmountType">
				<xs:annotation>
					<xs:documentation>The per-minute international charge</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="roamingCharge" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>The per-minute roaming charge</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="averageCharge" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>The average per-minute charge.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nationalRoamingCharge" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>The per minute national roaming charge</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="longDistanceMinutes" type="tns:MinutesIncludedQuantityType">
				<xs:annotation>
					<xs:documentation>Number of long distance minutes included</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="intlRoamingCharge" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>The per-minute international roaming charge</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billingIncrement" type="tns:RecurringChargeIncrementType">
				<xs:annotation>
					<xs:documentation>Indicates the increment the recurring charge occurs</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wirelessInternetChargeRate" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>Data charge rate</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="familyMinutes" type="tns:MinutesIncludedQuantityType">
				<xs:annotation>
					<xs:documentation>Number of sharing minutes included</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="walkieTalkieMinutes" type="tns:MinutesIncludedQuantityType">
				<xs:annotation>
					<xs:documentation>Number of Direct Connect minutes included</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="walkieTalkieOverageChargeRate" type="tns:ChargeAmountType">
				<xs:annotation>
					<xs:documentation>Overage charge rate for Direct Connect usage</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitSmsIncluded" type="tns:UnitOfMeasureValueIntegerType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SMS messaging included in the associated plan</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeviceSaleInfoType">
		<xs:sequence>
			<xs:element name="deviceSaleType" type="tns:SubsidyIndicatorCodeType">
				<xs:annotation>
					<xs:documentation>Indicates  Device Sale Type  S-Subscidized                      N-Non Subscidized               B- Bring your Own Device    I- Installment Billing             L- Lease</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeviceInfoType">
		<xs:choice>
			<xs:element name="itemId" type="tns:ModelNumberType">
				<xs:annotation>
					<xs:documentation>Item id, or model id of the subscriber’s device.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="meid" type="tns:MobileEquipmentIdentifierType">
				<xs:annotation>
					<xs:documentation>MEID of the subscriber’s device.
Required if hsn is not provided.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hsn" type="tns:DeviceSerialNumberType">
				<xs:annotation>
					<xs:documentation>iDEN device serial number</xs:documentation>
