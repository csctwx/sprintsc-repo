					</xs:annotation>
				</xs:element>
				
<xs:element name="subscriberInfo" type="tns:SearchCriteriaType">
					
<xs:annotation>
						
<xs:documentation>Subscriber Information</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			
<xs:element name="mdnPasscode" type="tns:VoicemailPassword" minOccurs="0">
				
