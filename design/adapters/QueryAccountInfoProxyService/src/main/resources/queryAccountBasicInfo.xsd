			<xs:element name="billFormatType" type="tns:BillFormatTypeCodeEns" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Bill Format Type Code specifies either a Business or Consumer format for the BILLING ACCOUNT's BILLs.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isWriteOff" type="tns:BinaryExpression" minOccurs="0">
