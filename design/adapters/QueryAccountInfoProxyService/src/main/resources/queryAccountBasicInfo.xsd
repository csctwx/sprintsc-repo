	<xs:element name="queryAccountBasicInfo" type="tns:QueryAccountBasicInfo"/>
	<xs:element name="queryAccountBasicInfoResponse" type="tns:QueryAccountBasicInfoResponse"/>
	<!---->
	<xs:complexType name="QueryAccountBasicInfo">
		<xs:sequence>
			<xs:element name="info" type="tns:SearchCriteria"/>
			<xs:element name="basicInfo" type="tns:FetchBasicInfoType" minOccurs="0"/>
			<xs:element name="fetchBillingAddressInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the billing address of the BAN should be returned.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchBillingNameInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the BAN’s billing name information should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchHeaderInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the BAN’s essential information (attributes typically set at BAN initiation/creation) should be returned.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchLockBoxInfo" type="tns:FetchInformationFlag" minOccurs="0"/>
			<xs:element name="fetchMailingAddressInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the BAN’s Mailing address information should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchOtherInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the OtherInfo should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchOtherNameInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether the BAN’s non-mandatory information  should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchSubscriberCountInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether the number of Subscribers should be returned.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchShippingAddressInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the BANs Shipping address information should be returned.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchSplitBillInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the Split Bill code whould be returned.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchSecurityGroupInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether to return corporate security group id.
Note: Restricted for use by 5DN only.  NDA project PJ002395.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="uaiInfo" type="tns:FetchUaiInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Search criteria for UAI information.  Populate if UAI information should be returned.  Restricted for use by front-ends authorized for real-time data.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchPersonalizationAttributeInfo" type="tns:FetchInformationFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the BANs personalized marketing attributes should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QueryAccountBasicInfoResponse">
		<xs:sequence>
			<xs:element name="basicInfo" type="tns:BasicInfo" minOccurs="0"/>
			<xs:element name="billingAddressInfo" type="tns:AddressInfo" minOccurs="0"/>
			<xs:element name="billingNameInfo" type="tns:BillingNameInfo" minOccurs="0"/>
			<xs:element name="headerInfo" type="tns:HeaderInfo" minOccurs="0"/>
			<xs:element name="lockBoxInfo" type="tns:LockBoxInfo" minOccurs="0"/>
			<xs:element name="mailingAddressInfo" type="tns:AddressInfo" minOccurs="0"/>
			<xs:element name="otherInfo" type="tns:OtherInfo" minOccurs="0"/>
			<xs:element name="otherNameInfo" type="tns:OtherNameInfo" minOccurs="0"/>
			<xs:element name="shippingAddressInfo" type="tns:AddressInfo" minOccurs="0"/>
			<xs:element name="splitBillInfo" type="tns:SplitBillInfoType" minOccurs="0"/>
			<xs:element name="subscriberCountInfo" type="tns:SubscriberCountInfo" minOccurs="0"/>
			<xs:element name="securityGroupInfo" type="tns:SecurityGroupInfoType" minOccurs="0"/>
			<xs:element name="uaiList" type="tns:UaiListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A list of UaiInfo records can be returned.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="personalizationAttributeInfo" type="tns:PersonalizationAttributeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>0 to 1 can be returned.  Will only return if the fetchPersonlizationAttributeInfo was set to tue.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!---->
	<xs:complexType name="AddressInfo">
		<xs:sequence>
			<xs:element name="type" type="tns:AddressTypeCode">
				<xs:annotation>
					<xs:documentation>Address Type Code differentiates an address as either a Military address, Rural address, Street address, PO Box address or a Foreign address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetNumber" type="tns:StreetNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Number is a component of a USPS Posatal Address that holds the house or building number on a street.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="line1" type="tns:PostalAddressLine" minOccurs="0"/>
			<xs:element name="line2" type="tns:PostalAddressLine" minOccurs="0"/>
			<xs:element name="ruralRoute" type="tns:RuralRouteControlNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rural Route Control Number holds the rural road or highway identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ruralNumber" type="tns:RuralRouteBoxNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rural Route Box Number holds the number identifying a mail box on a rural route.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pobNumber" type="tns:PostOfficeBoxNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Post Office Box Number holds a United States Postal Service Post Office Box Number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetName" type="tns:StreetName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>'Street Name is the USPS Postal Address component holding the name of a street.  For example in,"123 Main Street","Main"would reside in Street Name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetDirection" type="tns:StreetDirection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Direction is the component of a USPS Posatal Address holding the compass bearing (North, East, South, West and various combinations and abbreviations) appearing before or after a Street Name.  e.g.: North Oak, South Oak, NorthWest Barry Road, Barry Road NW, Oak South, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetType" type="tns:StreetSuffix" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Suffix is the USPS Postal Address component holding the trailing part of the street name.  For example in,"123 Main Street","Street"would reside in Street Suffix.  Other values might be Cir, Vlvd, Rd, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="trailingDirection" type="tns:TrailingDirection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Direction is the component of a USPS Posatal Address holding the compass bearing (North, East, South, West and various combinations and abbreviations) appearing after a Street Name.  e.g.: North Oak, South Oak, NorthWest Barry Road, Barry Road NW, Oak South, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartment" type="tns:ApartmentNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Apartment Number is the USPS Postal Address component holding the identifier of the apartment within the context of a bilding's postal address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartmentType" type="tns:ApartmentDesignator" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Apartment Designator is an Address Component that holds a value identifying the address as a type of apartment.Examples:"APT""UNIT"Etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attention" type="tns:AttentionTextEns" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Attention Text attached to the mailing address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="city" type="tns:CityNameEns" minOccurs="0">
				<xs:annotation>
					<xs:documentation>City Name holds the textual string identifying a City.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="state" type="tns:UspsStateAbbreviation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>'A United States Postal Service (USPS) State Abbreviation Code is the set of characters that are selected from a State or Protectorate name and specified by the USPS to represent the name of a State or US Protectorate for the purpose of mail delivery.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="tns:Iso3CharCountryCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>'An International Standardization Organization (ISO) 3-Character Country Code is the set of three characters assigned by the ISO to identify a soverign nation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode" type="tns:UspsPostalCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>'USPS Postal Code contains the first 5 characters of the nine digit USPS Postal Code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zip4Code" type="tns:UspsPostalCodeExtension" minOccurs="0">
				<xs:annotation>
					<xs:documentation>'USPS Posta Code Extension contains the last 4 characters of the nine digit USPS Postal Code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geoCode" type="tns:GeographicCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of geocoding to perform.  Valid values: ExactAddress, CrossStreet, Zip, CityState, International.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isGeoCodeOverridden" type="tns:GeoCodeManualOverrideFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag to indicate if  geo code was Manually Overridden</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="effectiveDate" type="tns:AddressEffectiveFromDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the effective date of the address.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BasicInfo">
		<xs:sequence>
			<xs:element name="ban" type="tns:BillingAccountNumberEns">
				<xs:annotation>
					<xs:documentation>'Billing Account Number uniquely identifies an instance of Billing Account used to accrue charges for invoicing.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountType" type="tns:BillingAccountTypeCodeEns">
				<xs:annotation>
					<xs:documentation>Billing Account Type Code categorizes BILLING_ACCOUNT instances into pre-defined segments used primarily for Customer segmentation in General Ledger reporting and Collection treatment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountSubType" type="tns:BillingAccountSubTypeCode">
				<xs:annotation>
					<xs:documentation>Billing Account SubType Code further categorizes the Billing Account within the context of the assigned Billing Account Type Code used primarily for Customer segmentation in General Ledger reporting and collection treatment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountStatus" type="tns:BillingAccountStatusCodeEns">
				<xs:annotation>
					<xs:documentation>The account status.  

Values:
A - Active
F - Final
N - Non-paid disconnect
W - Write-off</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isDelinquent" type="tns:InCollectionFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>In Collection Indicator identifies the BILLING ACCOUNT as an account in collections.

Values:
T => TRUE, the account is in Collections 
F=> FALSE the account is NOT in collections</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="delinquencyDate" type="tns:EventDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when the account moved to Delinquent status. This will be sent only if the above element (isDelinquent) is true.

This is different from the debtAge element, which is calculated based on when the invoice was generated
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="prePayInd" type="tns:PrePaidTypeCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>PrePayInd indicates the Customer is required to pay for service in advance.

Valid Values:
Y - Indicates the account is a pre-paid account.
N – Not a pre-paid account.
S - SIM Pre-paid account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="prepaidBrandCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code that indicates whether the subscriber belongs to the Sprint Brand or the Boost Brand.  Values are:
BST => Boost
SPR => Sprint</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:BrandCode">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="accountStatusChangeDate" type="tns:BillingAccountStatusEffectiveFromDate">
				<xs:annotation>
					<xs:documentation>Billing Account Status Effective From Date holds the first calendar day the status was assigned to the Billing Account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountStatusChangeRsnCode" type="tns:BillingAccountStatusChangeReasonCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing Account Status Change Reason Code identifies the reason for the activity that modified the Billing Account's status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultCsa" type="tns:CommunicationServiceAreaId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Communication Service Area Identifier uniquely identifies an instance of a geographic area (CSA) served by a communication service provider, which can be broken down into regional, metropolitan, district and smaller sections called ''leaves.''</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultNetworkInd" type="tns:NetworkIndicatorCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Network Indicator Code specifies the network being used.  

Values:
I -> iDEN; 
C -> CDMA; 
B -> Both</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="largeBanInfo" type="tns:LargeBanInfo"/>
			<xs:element name="legacyAccountNumber" type="tns:AccountNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing Account Number uniquely identifies an instance of Billing Account used to accrue charges for invoicing.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode" type="tns:UspsPostalCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>USPS Postal Code contains the first 5 characters of the nine digit USPS Postal Code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalDue" type="tns:TotalPaymentDue" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total Due amount (A/R balance) – this includes the last invoice balance and current charges and current adjustments.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastPaymentDate" type="tns:LastPaymentReceivedDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Last Payment Date – if a payment has been received.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentAmount" type="tns:PaymentAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Payment amount is the amount of an invoice's charges for which payments have been applied.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currentBalance" type="tns:CurrentBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Current Balance as shown on front end screen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="agingBalance1" type="tns:AgingBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aging Balance (1-30) days as shown on Collection screen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="agingBalance2" type="tns:AgingBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aging Balance (31-60) days as shown on Collection screen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="agingBalance3" type="tns:AgingBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aging Balance (61-90) days as shown on Collection screen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="agingBalance4" type="tns:AgingBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aging Balance (91+) days as shown on Collection screen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dealerCode" type="tns:DealerIdentifierEns" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dealer Identifier uniquely identifies the the agent or dealer who handled the customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subMarket" type="tns:SubMarketCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Submarket is a geographic, economic, or specialized subdivision of a market.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billCycle" type="tns:BillingCycleCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing Cycle Code uniquely identifies the group of Billing Accounts batched together for load balancing in the billing process of the billing System the Billing Account is assigned to.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pinInd" type="tns:PinSecurityFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>pin Indicator is a boolean flag that indicates if the security pin in set for this customer or not</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="passCode" type="tns:BillingAcctPassword" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field is deprecated and should not be used. Other apis should be used to retrieve CPNI data

Billing Account Password contains the system access password the Account Holder or the External  Account Administrator used to gain entry into the system  in order to make changes to the Billing Account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startDate" type="tns:StartDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Start Date holds the calendar day an activity, undertaking or participation begins.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deliveryMethod" type="tns:BillDeliveryPreferenceCode" minOccurs="0">
