<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://integration.sprint.com/interfaces/searchAccountList/v1/searchAccountList.xsd" targetNamespace="http://integration.sprint.com/interfaces/searchAccountList/v1/searchAccountList.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0">
	
<!--Date		Changed by		Version			Change Description-->
	
<!--10/16/2008	Jennifer Carlton		1.1		Changed state field from optional to required-->
	
<!--12/16/2008	Jennifer Carlton		1.2		CS#300532  Changed checkNegatve field from required to optional in 2places-->
	
<!--12/16/2008	Jennifer Carlton		1.2		CS#300719  Changed exactMatch field from required to optional in 2places, changed firstName to required-->
	
<!--10/13/2009	Balu Galla     		1.3		CS#341824  Changed allStatus field from required to optional-->
	
<!--01/09/2012              Ace Cineus                             2.0                                           Increased maxLen restriction of DeviceSerialNbr from 18 to 20                  GBL12.2          PJ006784-->
	
<xs:element name="searchAccountList" type="tns:SearchAccountList"/>
	
<xs:element name="searchAccountListResponse" type="tns:SearchAccountListResponse"/>
	
<xs:complexType name="SearchAccountList">
		
<xs:sequence>
			
<xs:element name="blockNumber" type="tns:BlockNumber" minOccurs="0"/>
			
<xs:element name="info" type="tns:Info"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="SearchAccountListResponse">
		
<xs:sequence>
			
<xs:element name="totalAvailableBlocks" type="tns:BlockNumber">
				
<xs:annotation>
					
<xs:documentation>Total available blocks of BANs should be returned </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="currentBlock" type="tns:BlockNumber">
				
<xs:annotation>
					
<xs:documentation>Current Block value should be returned (Should be equal to the value specified in the blockNumber field of request structure)</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="banList" type="tns:BanList"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="AccTypeSubType">
		
<xs:sequence>
			
<xs:element name="accountType" type="tns:BillingAccountTypeCodeEns"/>
			
<xs:element name="accountSubType" type="tns:BillingAccountSubTypeCode" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="BanInfo">
		
<xs:sequence>
			
<xs:element name="ban" type="tns:BillingAccountNumberEns"/>
			
<xs:element name="accountType" type="tns:BillingAccountTypeCodeEns"/>
			
<xs:element name="accountSubType" type="tns:BillingAccountSubTypeCode"/>
			
<xs:element name="accountStatus" type="tns:AccountStatusCode">
				
<xs:annotation>
					
<xs:documentation>BAN Status – Tentative, Open, Suspend, Cancelled</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="isDelinquent" type="tns:AccountIsDelinquentFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Is the BAN in collections?</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="prepayInd" type="tns:PrepayTypeCode" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Flag indicating if BAN is a pre-pay type.  Y (Boost), N (No) or S (SIM prepaid).</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="banStatusChangeDate" type="tns:EffectiveFromDate" minOccurs="0"/>
			
<xs:element name="banStatusChangeReasonCode" type="tns:BillingAccountStatusChangeReasonCode" minOccurs="0"/>
			
<xs:element name="ssn" type="tns:SocialSecurityNumber" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Social Security Number of customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="taxId" type="tns:FederalEmployerIdentificationNumber" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Tax Identification number of the customer </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="dunsNumber" type="tns:DunsNumber" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>D and B number</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="driverLicenseInfo" type="tns:DriverLicenseInfoResponse" minOccurs="0"/>
			
<xs:element name="billingNameInfo" type="tns:BillingNameInfo"/>
			
<xs:element name="billingAddressInfo" type="tns:BillingAddressInfo"/>
			
<xs:element name="corpId" type="tns:CorporateBillingAccountNumberEns" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Coroporation Identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="isEquipmentBan" type="tns:EquipmentOnlyFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Indicates if this BAN is only for equipment sales</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="relatedBan" type="tns:BillingAccountNumberEns" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>This is the BAN associated with Equipment BAN </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="BanList">
		
<xs:sequence>
			
<xs:element name="negativeSsnTaxIdInd" type="tns:NegativeSuspendFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Indicatez whether the ssn/taxId was present in the negative_ssn table.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="banInfo" type="tns:BanInfo" minOccurs="0" maxOccurs="50"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="BillingAddressInfo">
		
<xs:sequence>
			
<xs:element name="addressType" type="tns:AddressTypeCode"/>
			
<xs:element name="addressLine1" type="tns:PrimaryAddressLine" minOccurs="0"/>
			
<xs:element name="addressLine2" type="tns:SecondaryAddressLine" minOccurs="0"/>
			
<xs:element name="attention" type="tns:AttentionText" minOccurs="0"/>
			
<xs:element name="city" type="tns:CityNameEns" minOccurs="0"/>
			
<xs:element name="state" type="tns:USPSStateAbbreviation" minOccurs="0"/>
			
<xs:element name="country" type="tns:ISO3CharCountryCode"/>
			
<xs:element name="zipCode" type="tns:USPSPostalCd" minOccurs="0"/>
			
<xs:element name="zip4Code" type="tns:USPSPostalCdExtension" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="BillingNameInfo">
		
<xs:sequence>
			
<xs:element name="nameType" type="tns:PersonBusinessIndicator">
				
<xs:annotation>
					
<xs:documentation>If individual name – P (private individual), otherwise D (double line business)</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="individualNameInfo" type="tns:IndividualNameInfo" minOccurs="0"/>
			
<xs:element name="businessNameInfo" type="tns:BusinessNameInfo" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="BusinessNameInfo">
		
<xs:sequence>
			
<xs:element name="businessName" type="tns:BusinessNameEns">
				
<xs:annotation>
					
<xs:documentation>Business Name of Business Customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="additionalLine" type="tns:BillingAccountNameString" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Additional Line for Ban Name</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="ContactPhoneNumberInfo">
		
<xs:sequence>
			
<xs:element name="phone" type="tns:MobileDirectoryNumberString">
				
<xs:annotation>
					
<xs:documentation>Telephone Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="extension" type="tns:PhoneExtension" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Telephone Extension Number </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="CorpInfo">
		
<xs:sequence>
			
<xs:element name="corpId" type="tns:CorporateBillingAccountNumberEns">
				
<xs:annotation>
					
<xs:documentation>Corporation Identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="accTypeSubType" type="tns:AccTypeSubType" minOccurs="0"/>
			
<xs:element name="banType" type="tns:BillingAccountCategoryCode">
				
<xs:annotation>
					
<xs:documentation>E - Equipment Ban, S- ServiceBan </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="DriverLicenseInfo">
		
<xs:sequence>
			
<xs:element name="driverLicenseNumber" type="tns:DriverLicenseNumber">
				
<xs:annotation>
					
<xs:documentation>Customer driving license number</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="state" type="tns:DriverLicenseStateCode">
				
<xs:annotation>
					
<xs:documentation>State of the Driving License.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="DriverLicenseInfoResponse">
		
<xs:sequence>
			
<xs:element name="driverLicenseNumber" type="tns:DriverLicenseNumber">
				
<xs:annotation>
					
<xs:documentation>Customer driving license number</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="state" type="tns:DriverLicenseStateCode">
				
<xs:annotation>
					
<xs:documentation>State of the Driving License</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="driverLicenseExpirationDate" type="tns:DriverLicenseExpirationDate" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Expiration date of the license</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="FirstNameInfo">
		
<xs:sequence>
			
<xs:element name="firstName" type="tns:FirstNameEns">
				
<xs:annotation>
					
<xs:documentation>First Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="exactMatch" type="tns:ExactMatchCode" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>E – Exact Match
S – Starts With </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="IndividualNameInfo">
		
<xs:sequence>
			
<xs:element name="firstName" type="tns:FirstNameEns" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>First Name of individual Customer </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="middleName" type="tns:MiddleNameEns" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Middle Initial name of individual customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="lastName" type="tns:LastNameEns">
				
<xs:annotation>
					
<xs:documentation>Last name of individual user or Business Name of Business Customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="title" type="tns:TitleEns" minOccurs="0"/>
			
<xs:element name="generation" type="tns:GenerationEns" minOccurs="0"/>
			
<xs:element name="additionalLine" type="tns:AdditionalTitleEns" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="Info">
		
<xs:sequence>
			
<xs:element name="allStatus" type="tns:FetchInformationFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>false - Records with following ban statuses shall only be returned.
Opened (O)
Tentative (T)
Suspended(S)

true – All records irrespective of status shall be returned

By default only records of  the three statuses Opened (O), Tentative(T) and Suspended(S) would be returned.
Records with status Closed(C)/Cancelled (N) will not be returned.
</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="searchCriteria" type="tns:SearchCriteria"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="LastBusinessNameInfo">
		
<xs:sequence>
			
<xs:element name="lastBusinessName" type="tns:BusinessNameEns">
				
<xs:annotation>
					
<xs:documentation>Last Name or Business Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="exactMatch" type="tns:ExactMatchCode" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>E – Exact Match
S – Starts With </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="NameInfo">
		
<xs:sequence>
			
<xs:element name="type" type="tns:NameSearchTypeCode">
				
<xs:annotation>
					
<xs:documentation>BillingName(B), User(U), 
Both(A) (Default)</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="lastBusinessNameInfo" type="tns:LastBusinessNameInfo"/>
			
<xs:element name="firstNameInfo" type="tns:FirstNameInfo" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="SearchCriteria">
		
<xs:choice>
			
<xs:element name="ssnInfo" type="tns:SsnInfo"/>
			
<xs:element name="driverLicenseInfo" type="tns:DriverLicenseInfo"/>
			
<xs:element name="taxIdInfo" type="tns:TaxIdInfo"/>
			
<xs:element name="dunsNumber" type="tns:DunsNumber">
				
<xs:annotation>
					
<xs:documentation>Search based on D and B number</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="subscriberId" type="tns:SubscriptionIdentifierEns"/>
			
<xs:element name="deviceSerialNumber" type="tns:SubscriberDeviceSerialNumber">
				
<xs:annotation>
					
<xs:documentation>Any IMEI, MSN, SIM or ESN number of  a subscriber (all statuses). ESN can be in both decimal or hexadecimal format. </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="mdn" type="tns:MobileDirectoryNumberString">
				
<xs:annotation>
					
<xs:documentation>PTN Number of a subscriber </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="ufmi" type="tns:UrbanFleetMemberIdentifierString">
				
<xs:annotation>
					
<xs:documentation>Urban Id, Fleet Id, Member Id (e.g., 128*301*12331) </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="ipAddress" type="tns:InternetProtocolAddressString">
				
<xs:annotation>
					
<xs:documentation>The IP address of a subscriber (e.g., 10.11.12.13) </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="nai" type="tns:NetworkAccessIdentifierEns">
				
<xs:annotation>
					
<xs:documentation>The Network Access Indicator (e.g. username@domain) </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="nameInfo" type="tns:NameInfo"/>
			
<xs:element name="aslAdminMdn" type="tns:MobileDirectoryNumberString">
				
<xs:annotation>
					
<xs:documentation>PTN of the administrator of the ASL account</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="contactNumberInfo" type="tns:ContactPhoneNumberInfo"/>
			
<xs:element name="corpInfo" type="tns:CorpInfo"/>
		</xs:choice>
	</xs:complexType>
	
<xs:complexType name="SsnInfo">
		
<xs:sequence>
			
<xs:element name="ssn" type="tns:SocialSecurityNumber">
				
<xs:annotation>
					
<xs:documentation>Social Security Number of customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="checkNegative" type="tns:FetchInformationFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>True - search for the ssn in the negative_ssn table.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="TaxIdInfo">
		
<xs:sequence>
			
<xs:element name="taxId" type="tns:FederalEmployerIdentificationNumber">
				
<xs:annotation>
					
<xs:documentation>Tax Identification number of the customer </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="checkNegative" type="tns:FetchInformationFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>True - search for the taxId in the negative_ssn table.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<!--     MVM simple types-->
	
<xs:simpleType name="AccountNumberInt">
		
<xs:annotation>
			
<xs:documentation>Account Number uniquely identifies an Account.  The Account may be a Financial Account, a Customer Account, Billing Account, etc.</xs:documentation>
		</xs:annotation>
		
<xs:restriction base="xs:int"/>
	</xs:simpleType>
	
<xs:simpleType name="AccountNumberString">
		
<xs:annotation>
			
<xs:documentation>Account Number uniquely identifies an Account.  The Account may be a Financial Account, a Customer Account, Billing Account, etc.</xs:documentation>
		</xs:annotation>
		
<xs:restriction base="xs:string">
			
<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="AccountStatusCode">
		
<xs:annotation>
			
<xs:documentation>Account Status Code is used to indicate the status of the account.</xs:documentation>
		</xs:annotation>
		
<xs:restriction base="tns:StatusCode">
			
<xs:maxLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="AdditionalTitle">
		
<xs:restriction base="xs:string">
			
<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="AdditionalTitleEns">
		
<xs:restriction base="xs:string">
			
<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="AddressTypeCode">
		
<xs:annotation>
			
<xs:documentation>Address Type Code distinguishes an address as either a Military address, Rural address, Street address, PO Box address or a Foreign address.</xs:documentation>
		</xs:annotation>
		
<xs:restriction base="tns:TypeCode">
			
<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="AttentionText">
		
<xs:annotation>
			
<xs:documentation>Attention Text of the mailing address.</xs:documentation>
		</xs:annotation>
		
<xs:restriction base="xs:string">
			
<xs:maxLength value="25"/>
		</xs:restriction>
	</xs:simpleType>
	
<xs:simpleType name="BillingAccountNumberEns">
		
<xs:annotation>
			
<xs:documentation>Billing Account Number uniquely identifies an instance of Billing Account used to accrue charges for invoicing.</xs:documentation>
		</xs:annotation>
		
<xs:restriction base="tns:AccountNumberInt">
			
<xs:totalDigits value="9"/>
		</xs:restriction>
	</xs:simpleType>
	
