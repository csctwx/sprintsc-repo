<xs:element name="queryPrepaidUsageDetails" type="tns:QueryPrepaidUsageDetailsRequestType"/>
	
<xs:element name="queryPrepaidUsageDetailsResponse" type="tns:QueryPrepaidUsageDetailsResponseType"/>
	
<xs:complexType name="QueryPrepaidUsageDetailsRequestType">
		
<xs:sequence>
			
<xs:element name="subscriberInfo" type="tns:SearchCriteriaType">
				
<xs:annotation>
					
<xs:documentation>Subscriber Info</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="applicationCallingInfo" type="tns:ApplicationCallingInfoType">
				
<xs:annotation>
					
<xs:documentation>The prepaid Usage Details Request Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="prepaidUsageDetailsRequestInfo" type="tns:PrepaidUsageDetailsRequestInfoType">
				
<xs:annotation>
					
<xs:documentation>Structure contains calling application information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="QueryPrepaidUsageDetailsResponseType">
		
<xs:sequence>
			
<xs:element name="prepaidUsageDetailsHistoryInfoList" type="tns:PrepaidUsageDetailsHistoryListType">
				
<xs:annotation>
					
<xs:documentation>Transaction history of the sub, including usage and financial info for a prepaid subscriber</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="alternateAuthenticationDate" type="tns:EventDateType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The Alternate Authentication suspend date</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="paginationResultInfo" type="tns:PaginationResultInfoType">
				
<xs:annotation>
					
<xs:documentation>Return the pagination info of the response</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="ApplicationCallingInfoType">
		
<xs:sequence>
			
<xs:element name="accessId" type="tns:ApplicationIdentifierType">
				
<xs:annotation>
					
<xs:documentation>Calling application Id</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="EventCategoryInfoType">
		
<xs:sequence>
			
<xs:element name="eventCategory" type="tns:ServiceCategoryNameType">
				
<xs:annotation>
					
<xs:documentation>The event category.
The valid values are defined in GENERIC_CODES.
(GEN_TYPE will be SERVICE_CATEGORY)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="EventCategoryListType">
		
<xs:sequence>
			
<xs:element name="EventCategoryInfo" type="tns:EventCategoryInfoType" maxOccurs="unbounded">
				
<xs:annotation>
					
<xs:documentation>Event type category information (most of the time, it will have only one value)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="MediaInfoType">
		
<xs:sequence>
			
<xs:element name="mediaType" type="tns:ContentMediaTypeCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Media type that was used in an MMS event
Valid Values :
P - Picture
A - Audio
V - Video
T - Text</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="mediaQuantity" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Media quantity that was used in an MMS event.</xs:documentation>
				</xs:annotation>
				
<xs:simpleType>
					
<xs:restriction base="tns:RowCountNumberType">
						
<xs:totalDigits value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
<xs:element name="mediaVolume" type="tns:MediaUnitOfMeasureValueType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Media volume that was used in an MMS event.
Will be measured in Bytes.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="PaginationInfoType">
		
<xs:sequence>
			
<xs:element name="pageNumber" type="tns:PageNumberType">
				
<xs:annotation>
					
<xs:documentation>Requested page number:
Page number = 1 returns first page's data and total count
Page number &gt; 1 return data of requested page but total count is not returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="pageBufferSize" type="tns:PageBufferRowCountNumberType">
				
<xs:annotation>
					
<xs:documentation>Requested page buffer size. Maximum page buffer size is 600.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="PaginationResultInfoType">
		
<xs:sequence>
			
<xs:element name="moreRows" type="tns:RowCountNumberType">
				
<xs:annotation>
					
<xs:documentation>Indication of whether there are more rows to retrieve (1 – there are more rows to retrieve; 0 – no more rows to retrieve)</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="rowCount" type="tns:RowCountNumberType">
				
<xs:annotation>
					
<xs:documentation>Number of usage detail records on the current page</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="totalCount" type="tns:RowCountNumberType">
				
<xs:annotation>
					
<xs:documentation>Total number of usage detail records. Value will be 0 if:
PaginationInfo.pageNumber &gt; 0 OR Total count = 0 
</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="PrepaidUsageDetailsHistoryInfoType">
		
<xs:sequence>
			
<xs:element name="prepaidUsageDetailsInfo" type="tns:PrepaidUsageDetailsInfoType">
				
<xs:annotation>
					
<xs:documentation>Transaction’s detailed information</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="eventCategoryList" type="tns:EventCategoryListType">
				
<xs:annotation>
					
<xs:documentation>The event’s Category list</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="eventStartTime" type="tns:EffectiveFromDateTimeType">
				
<xs:annotation>
					
<xs:documentation>The event's start time.
Will be in format:yyyymmddhhmmss</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="callDirection" type="tns:CallDirectionIdentifierType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The direction of the call - incoming or outgoing.
Incoming/Outgoing/empty</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="calledNumber" type="tns:CalledNumberType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The number the call was made to.
For incoming calls, ‘Incoming’ will be presented.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="transactionDescription" type="tns:TransactionDescriptionType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The description of the transaction</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="chargeAmount" type="tns:ChargeAmountType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The calculated total charge amount of the event.
The chargeAmount will not be returned for non-monetary adjustments.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="remainingBalance" type="tns:BalanceAmountType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The remaining balance amount that the customer has after processing the event.
remainingBalance will not be returned for non-monetary adjustments</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="rechargeMethod" type="tns:RechargeMethodTypeCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The type of activity that is represented by the recharge event (for instance: increase adjustment, RC to be reduced from balance, RC to be reduced from credit card) </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="rechargeChannel" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The source application that initiated the recharge event</xs:documentation>
				</xs:annotation>
				
<xs:simpleType>
					
<xs:restriction base="tns:ApplicationIdentifierType">
						
<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
<xs:element name="allowanceIndicator" type="tns:BinaryExpressionType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Indicator whether the event consumed an allowance or not.
Valid values are: true/false  if there is no value, the tag will not be sent
</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="url" type="tns:UniformResourceLocatorType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The website address associated with the content provider.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="tollFreeNumber" type="tns:PhoneNumberType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The contact information associated with the content provider.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="shortContentType" type="tns:SettlementTypeCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Defines categories of on-line purchases and whether or not they are Sprint charges, third 3rd party tax inclusive charges, or third3rd party tax exclusive charges.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="shortCode" type="tns:PremiumShortMessageServiceProductIdentifierType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>a five to nine digital numerical string for a premium SMS charge that identifies a premium SMS product. </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="serviceId" type="tns:SettlementTypeCodeType">
				
<xs:annotation>
					
<xs:documentation>Attribute that defines a content purchase as Sprint, Third3rd Party Tax Exclusive, or Third3rd Party Tax Inclusive in AmdocsAMDOCs. PSI and AMS calls this the feature code.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="PrepaidUsageDetailsHistoryListType">
		
<xs:sequence>
			
<xs:element name="prepaidUsageDetailsHistoryInfo" type="tns:PrepaidUsageDetailsHistoryInfoType" maxOccurs="unbounded">
				
<xs:annotation>
					
<xs:documentation>Transaction history of the sub, including usage and financial info for a prepaid subscriber</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="PrepaidUsageDetailsInfoType">
		
<xs:sequence>
			
<xs:element name="usageInfo" type="tns:UsageInfoType">
				
<xs:annotation>
					
<xs:documentation>Usage information
For VMU, will contain data and text overage charges</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="transactionChargeInfo" type="tns:TransactionChargeInfoType">
				
<xs:annotation>
					
<xs:documentation>Charge information 
For VMU, has only voice-related fields. For data/text charges, the eventProcessTime, feeType (where applicable), and airTimeChargeAmount are applicable.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="reboostInfo" type="tns:ReboostInfoType">
				
<xs:annotation>
					
<xs:documentation>Re-boost Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="mediaInfo" type="tns:MediaInfoType">
				
<xs:annotation>
					
<xs:documentation>Media Information
For VMU will not be populated</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="PrepaidUsageDetailsRequestInfoType">
		
<xs:sequence>
			
<xs:element name="fromDate" type="tns:EffectiveFromDateType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The time range for retrieving the account transactions</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="paginationInfo" type="tns:PaginationInfoType">
				
<xs:annotation>
					
<xs:documentation>Requested pagination information </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="ReboostInfoType">
		
<xs:sequence>
			
<xs:element name="rechargeStatus" type="tns:RechargeStatusCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Status
Will return only if cancelled</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="paymentMethodId" type="tns:PaymentCardLast4DigitsType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Related to Recharge events. Displays the last four digits of the credit card number.
For CC, displays the last four digits of the credit card used in the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="paymentMethodType" type="tns:PaymentCardTypeCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Credit Card type
AM - American Express
DC - Diners Club
DI - Discover Network
MC - MasterCard 
VS - Visa
WP - Write-off Payment
PL - PayPal</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="SearchCriteriaType">
		
<xs:choice>
			
<xs:element name="subscriberId" type="tns:SubscriptionIdentifierType">
				
<xs:annotation>
					
<xs:documentation>Subscription Identifier uniquely identifies an instance of SUBSCRIPTION</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="mdn" type="tns:MobileDirectoryNumberStringType">
				
<xs:annotation>
					
<xs:documentation>'Mobile Directory Number (MDN) is the actual phone number one would dial to reach a specific mobile phone.
Prior to Wireless Number Portability, MDN was the same number as the Mobile Identification Number (MIN) for many mobile phones. But now that MDNs can be ported (moved) to other carriers, MDN and MIN will be different for ported numbers.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="equipmentId" type="tns:EquipmentSerialNumberStringType">
				
<xs:annotation>
					
<xs:documentation>Equipment Serial Number holds the identifier used by the device to communicate (broadcast) its identity to the wireless mobile network.  
Network Device Identifier will contain one of the following:
Electronic Serial Number (ESN);
Mobile Equipment IDentifier (MEID);
International Mobile Equipment Identfier (IMEI);
Subscriber Identify Module (SIM).</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="ufmi" type="tns:UrbanFleetMemberIdentifierStringType">
				
<xs:annotation>
					
<xs:documentation>Urban Fleet Member Identifier (UFMI) is the combinaiton of the Urban Area Id, Fleet Id, and Member Id representing an address in a format recognized within iDEN Push-To-Talk (PTT) or Direct Connect. For example, a UFMI number of 500•600•700, translates to 500 is the Urban Area ID, 600 is the Fleet, and 700 is the Member.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="ipAddress" type="tns:InternetProtocolAddressStringType">
				
<xs:annotation>
					
<xs:documentation>An Internet Protocol Address is the numerical address by which a location in the Internet is identified. Computers on the Internet use IP addresses to route traffic and establish connections among themselves.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="imsi" type="tns:InternationalMobileSubscriberIdentityStringType">
				
<xs:annotation>
					
<xs:documentation>International Mobile Subscriber Identity (IMSI) is a unique number associated with all GSM and Universal Mobile Telecommunications System (UMTS) network mobile phone users. It is stored in the Subscriber Identity Module (SIM) inside the phone and is sent by the phone to the network. It is also used to acquire other details of the mobile in the Home Location Register (HLR) or as locally copied in the Visitor Location Register. In order to avoid the subscriber being identified and tracked by eavesdroppers on the radio interface, the IMSI is sent as rarely as possible and a randomly generated TMSI is sent instead.
The IMSI is used in any mobile network that interconnects with other networks, in particular CDMA and EVDO networks as well as GSM nets. This number is provisioned in the phone directly or in the R-UIM card (a CDMA analogue equivalent to a SIM card in GSM)
An IMSI is usually 15 digits long, but can be shorter (for example MTN South Africa's IMSIs are 14 digits). The first 3 digits are the Mobile Country Code (MCC), and is followed by the Mobile Network Code (MNC), either 2 digits (European standard) or 3 digits (North American standard). The remaining digits are the Mobile Subscriber Identification Number (MSIN) within the network's customer base.
The IMSI conforms to the International Telecommunication Union (ITU) E.212 numbering standard</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="nai" type="tns:NetworkAccessIdentifierStringType">
				
<xs:annotation>
					
<xs:documentation>Network Access Identifier (NAI) is a standard way of identifying users who request access to a network. 

The standard syntax is"user@realm". </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="additionalImsi" type="tns:InternationalMobileSubscriberIdentityStringType">
				
<xs:annotation>
					
<xs:documentation>Additional IMSI of the subscriber to be updated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="hppttId" type="tns:HighPerformancePushToTalkIdentifierStringType">
				
<xs:annotation>
					
<xs:documentation>The subscriber's HPPTT ID. The ID will be constructed from username@domain, where the username will be a PTN or UFMI-based number.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	
<xs:complexType name="TransactionChargeInfoType">
		
<xs:sequence>
			
<xs:element name="eventProcessTime" type="tns:EventDateTimeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The event's GMT time
In format : yyyyymmddhhmmss</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="chargedDuration" type="tns:CallDurationSecondsType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The duration portion of the event that was charged (was not covered by free allowances). Measured in seconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="freeAllowedDuration" type="tns:CallDurationSecondsType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The event's duration that is being allocated an allowance and, therefore, is not charged. Measured in seconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="feeType" type="tns:ChargeTypeCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Type of daily fee</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="dailyFeeCharge" type="tns:ChargeAmountType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Charge for events triggering a daily fee</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="airTimeChargeAmount" type="tns:ChargeAmountType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Charge for the air time of the call. 
This field is applicable for VMU.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="internationalSurcharge" type="tns:ChargeAmountType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Additional charge for international calls that contributes to the total charge of the event</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="additionalCharge" type="tns:ChargeAmountType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The total of additional charges that contributed to the total charge of the event (for example, charge for usage is greater than that defined in the offer)</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="longDistanceSurcharge" type="tns:ChargeAmountType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Surcharge for Long Distance calls</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="UsageInfoType">
		
<xs:sequence>
			
<xs:element name="serviceType" type="tns:EventServiceTypeNameType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Identifies the type of service that the event represents (such as Voice or Text messaging).
For VMU, can be used to differentiate data or voice or SMS overage.
See appendix for valid values.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="serviceFilter" type="tns:EventServiceFilterStringType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>A string that holds information about the type of rated event (for instance, if it is national/international, mobile to other/mobile, mobile originated/terminated call).</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="socCode" type="tns:ServiceOrderCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Soc code</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="socDescription" type="tns:ServiceOrderDescriptionType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Soc Description</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="calledCountryCode" type="tns:CalledCountryNameType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The Country code of the country dialed by the subscriber. For example:UK,USA,UY</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="ratedPeriod" type="tns:UsagePeriodTypeCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The name of the period on which the event was rated in a period sensitive rate scheme.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="timeZone" type="tns:TimeZoneCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The time zone of the switch in which the call was captured </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="roundedDuration" type="tns:CallDurationSecondsType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The rounded duration of the call according to the rounding factor and rounding method (up/down/math), which were defined as the PIT (pricing Item Type) parameters</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="companyName" type="tns:CompanyNameType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Displays the name of the company associated with the transaction</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="productId" type="tns:ProductIdentifierType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Product ID of a Content event</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="productName" type="tns:ProductNameType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Product name of a Content event</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="productDescription" type="tns:ProductDescriptionType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Product description of a Content event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="allowanceDescription" type="tns:AllowanceTypeDescriptionType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Description of the allowance the event consumed.
Will be populated only for allowance based events</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="comment" type="tns:NoteTextType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>A descriptive comment of a non-monetary adjustment</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="uom" type="tns:UnitOfMeasureNameType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Unit Of Measure - describes the type of event. A duration event will usually have 'seconds', a text messaging event will have 'occurrence'. </xs:documentation>
				</xs:annotation>
			</xs:element>
			
