	<xs:complexType name="ValidateAccountInfoRequestType">
		<xs:sequence>
			<xs:element name="accountInfo" type="tns:AccountInfoType">
				<xs:annotation>
					<xs:documentation>This structure contains customer’s  account information required as searchCriteria for this API</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="singleSubscriberInd" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This flag indicates rather API should return only associated subscriber data in the response. This flag is used in conjunction with reference-ptn, sim, meid fields. (under accountInfo structure)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="authenticationInfo" type="tns:AuthenticationInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This structure contains information for authenticating the customer’s credentials</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="requestedDeviceList" type="tns:RequestedDeviceListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For customers requesting the available pricing for  select phone models. Returns up to 5 modelIds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnBogxInd" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This flag indicates rather Bogx info should be returned or not in the response.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="upgradeDeviceInfo" type="tns:UpgradeDeviceInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about the upgrade device (when applicable) If this is provided along with subscriber identifier OVM will validate if there is a need for plan change with the device swap</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchRebateInfoInd" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates 'true' if the rebate plan info to be fetched in the reponse</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="checkLoanEligibilityInd" type="tns:ProcessRequiredFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates if check loan eligibility needs to be performed.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ValidateAccountInfoResponseType">
		<xs:sequence>
			<xs:element name="accountStatus" type="tns:BillingAccountStatusCodeType">
				<xs:annotation>
					<xs:documentation>Status of account, valid values include: ACTIVE, TENTATIVE, CLOSED, CANCELLED, and SUSPENDED</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountStatusDate" type="tns:BillingAccountStatusEffectiveFromDateTimeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date that the account status last changed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ban" type="tns:AccountNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Sprint Nextel account number.
Pattern: \d{1,10}</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="numberOfHandsetsApproved" type="tns:AllowedHandsetQuantityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number of handsets approved on the account</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountSubscribertInfo" type="tns:AccountSubscriberInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure that contains Account Information details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billingNameInfo" type="tns:BillingNameInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure that contains Customer’s billing name details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billingAddressInfo" type="tns:BillingAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing Address</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountBalance" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Account Balance
For Boost vendor only. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentDueDate" type="tns:PaymentDueDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Payment Due Date
For Boost vendor only. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="balanceExpirationDate" type="tns:ExpirationDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Balance Expiration Date
For Boost vendor only. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="advancePaymentInd" type="tns:BinaryExpressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates if 
For Boost vendor only. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccountInfoType">
		<xs:choice>
			<xs:element name="ban" type="tns:BillingAccountNumberType">
				<xs:annotation>
					<xs:documentation>The Billing Account Number.  Required if the referenceMdn is not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mdn" type="tns:MobileDirectoryNumberStringType">
				<xs:annotation>
					<xs:documentation>Sprint-Nextel cell phone number of the customer.  Required if the accountNumber is not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deviceSerialNumber" type="tns:SubscriberDeviceSerialNumberType">
				<xs:annotation>
					<xs:documentation>Customer’s handset ESN number from their CDMA device. Required if the accountNumber, referenceMdn, or simId is not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="macId" type="tns:MediumAccessControlAddressType">
				<xs:annotation>
					<xs:documentation>Customer’s handset MAC-ID number from their Airave device. Required if the accountNumber, referenceMdn, meidId or simId is not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="iccId" type="tns:UniversalIntegratedCircuitCardIdentifierType">
				<xs:annotation>
					<xs:documentation>Integrated Circuit Card Identifier is a unique identifier of the smart card used in mobile terminals in CDMA networks.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="AccountSubscriberInfoType">
		<xs:sequence>
			<xs:element name="currentNumberOfSubscribers" type="tns:SubscriberCountType">
				<xs:annotation>
					<xs:documentation>Number of subscribers on the associated account</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberList" type="tns:SubscriberListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure that contains subscriber information details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="allowedLoanCount" type="tns:LoanCountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the allowed loans on the given account (Max allowed - Open loans). </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="studentLineMaxLimit" type="tns:LineQuantityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>maximum Student line allowed in the account. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="loanEligibilityInd" type="tns:EligibilityFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates loan eligibility of the account </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddOnSocInfoType">
		<xs:sequence>
			<xs:element name="optionName" type="tns:OptionNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer friendly name of the associated feature</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="optionCode" type="tns:OptionCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Service offer code representing the add on option.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="optionPrice" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Option price</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddOnSocListType">
		<xs:sequence>
			<!-- addOnSocInfo needs to be an exact match for queryAvailablePlansResponse.addOnSocList.addOnSocInfo, they point to the same OVM common structure / fields-->
			<xs:element name="addOnSocInfo" type="tns:AddOnSocInfoType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>contains add on soc (options) details for each subscriber</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AuthenticationInfoType">
		<xs:sequence>
			<xs:element name="secPin" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer’s security pin number. Optionally used in conjunction with reference-ptn, sim, meid to return only associated subscriber data in the response.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:PersonalIdentificationNumberType">
						<xs:minLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="secAnswer" type="tns:SecurityAnswerType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer’s security answer. Optionally provided if response is shown before customer has been authenticated and doesn’t know PIN</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerIdentificationInfo" type="tns:CustomerIdentificationInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This structure contains customer’s  SSN or TAX ID information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer’s billing last name on account can be optionally provided</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:LastNameType">
						<xs:minLength value="2"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="serviceAreaZipInfo" type="tns:ServiceAreaZipInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer’s billing zip code on account can be optionally provided in order to provide rebate eligibility online in conjunction with phone number</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BillingAddressType">
		<xs:sequence>
			<xs:element name="streetAddress1" type="tns:PostalAddressLine1Type"/>
			<xs:element name="streetAddress2" type="tns:PostalAddressLine2Type" minOccurs="0"/>
			<xs:element name="city" type="tns:CityNameType"/>
			<xs:element name="stateCode" type="tns:UspsStateAbbreviationType"/>
			<xs:element name="zipCode" type="tns:UspsPostalCodeType">
				<xs:annotation>
					<xs:documentation>Zip code of service area.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zip4Code" type="tns:UspsPostalCodeExtensionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Zip extension code.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BillingNameInfoType">
		<xs:sequence>
			<xs:element name="firstName" type="tns:FirstNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>First customer’s name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastBusinessName" type="tns:LastNameType">
				<xs:annotation>
					<xs:documentation>Last business name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalTitle" type="tns:AdditionalTitleType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer’s additional title. Example: Mr, Mrs, Ms</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BundleInfoType">
		<xs:sequence>
			<xs:element name="bundleType" type="tns:PrimarySecondaryTypeCodeType">
				<xs:annotation>
					<xs:documentation>Indicates the type of bundle the associated price plan is: primary or secondary. Values: P, S</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bundleGroupList" type="tns:BundleGroupListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of bundle group numbers</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BundleGroupInfoType">
		<xs:sequence>
			<xs:element name="bundleGroupNumber" type="tns:BundleIdentifierType">
				<xs:annotation>
					<xs:documentation>Indicates which group or groups the bundle price plan is associated with.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BundleGroupListType">
		<xs:sequence>
			<xs:element name="bundleGroupInfo" type="tns:BundleGroupInfoType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CurrentEligibilityInfoType">
		<xs:sequence>
			<xs:element name="eligibilityDescription" type="tns:EligibilityStatementDescriptionType">
				<xs:annotation>
					<xs:documentation>Describes current eligibility if eligible = true</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CurrentEligibilityListType">
		<xs:sequence>
			<xs:element name="currentEligibilityInfo" type="tns:CurrentEligibilityInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Structure that provides eligibility description</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomerIdentificationInfoType">
		<xs:choice>
			<xs:element name="ssn" type="tns:SsnLast4DigitsType">
				<xs:annotation>
					<xs:documentation>This is the customer’s last four digits of their ssn if they are a consumer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="federalTaxId" type="tns:TaxIdentificationNumberLast4DigitsType">
				<xs:annotation>
					<xs:documentation>This is the customer’s last four digits of their tax id if they are a business.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="DataAllowanceInfoType">
		<xs:sequence>
			<xs:element name="dataAllowanceType" type="tns:AllowanceTypeCodeType">
				<xs:annotation>
					<xs:documentation>Type of allowance</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataAllowance" type="xs:double">
				<xs:annotation>
					<xs:documentation>Subscriber allowance quantity</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DataAllowanceListType">
		<xs:sequence>
			<xs:element name="dataAllowanceInfo" type="tns:DataAllowanceInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Data Allowance Information</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DevicePricingInfoType">
		<xs:sequence>
			<xs:element name="itemId" type="tns:ModelNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Model identifier of the  device</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modelPrice" type="xs:double">
				<xs:annotation>
					<xs:documentation>Device price</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractTerm" type="tns:NumberOfContractMonthsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contract commitment required for associated model price</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DevicePricingListType">
		<xs:sequence>
			<xs:element name="subscriberDevicePricingInfo" type="tns:DevicePricingInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Structure that provides Device Pricing details</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EligibleTierInfoType">
		<xs:sequence>
			<xs:element name="eligibleTierDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date that subscriber becomes eligible for associated tier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="eligibleTierLevel" type="tns:TierLevelType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies subscriber tier level for commissioning purposes</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EligibleTierListType">
		<xs:sequence>
			<xs:element name="eligibleTierInfo" type="tns:EligibleTierInfoType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Structure that describes eligible Tier</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EquipmentGroupInfoType">
		<xs:sequence>
			<xs:element name="groupDescription" type="tns:BuyOneGetManyEquipmentGroupDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Group Description</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="groupId" type="tns:BuyOneGetManyEquipmentGroupIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Group ID (will return if a group is defined and not just a unique item)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="itemList" type="tns:ItemInfoListType">
				<xs:annotation>
					<xs:documentation>List of item information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="action" type="tns:BuyOneGetManyActionTypeCodeType">
				<xs:annotation>
					<xs:documentation>B - Buy; G - Get </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quantity" type="tns:OfferItemQuantityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number of items to purchase </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="offerLineCode" type="tns:OfferLineItemIdentifierType">
				<xs:annotation>
					<xs:documentation>The offer line code </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="price">
				<xs:annotation>
					<xs:documentation>Net BOGX Price </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:PriceAmountType">
						<xs:totalDigits value="9"/>
						<xs:fractionDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="additionalDiscount" type="tns:DiscountAmountType">
				<xs:annotation>
					<xs:documentation>The additional discount defined in EQUIPMENT_OFFER_PRICING table </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commitmentTerm" type="tns:CommitmentPeriodMonthQuantityType">
				<xs:annotation>
					<xs:documentation>Commitment term in months. Zero for no commitment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subGroup" type="tns:EquipmentSubGroupCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A sub group is a pairing of multiple entries in the EQP_OFFER_PRICING table which are part of a logical group within a Flexible Mix and Match offer.  Each item or group in the sub group can have a different price. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subGroupQuantity" type="tns:EquipmentSubgroupItemCountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Equipment sub group quantity </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyPrice">
				<xs:annotation>
					<xs:documentation>The buy price is price+additionalDiscount</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:PriceAmountType">
						<xs:totalDigits value="9"/>
						<xs:fractionDigits value="2"/>
