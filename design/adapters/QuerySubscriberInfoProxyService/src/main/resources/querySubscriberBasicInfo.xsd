	<xs:element name="querySubscriberBasicInfo" type="tns:QuerySubscriberBasicInfoType"/>
	<xs:element name="querySubscriberBasicInfoResponse" type="tns:QuerySubscriberBasicInfoResponseType"/>
	<xs:complexType name="QuerySubscriberBasicInfoType">
		<xs:sequence>
			<xs:element name="info" type="tns:SearchCriteriaType">
				<xs:annotation>
					<xs:documentation>Structure containing choice of subscriber search criteria</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchDetailInfo" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether to request Detail Info associated to the request structure.
If true, perform request for Detail Info. If false, request the associated information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchAddressInfo" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether to request Address Info associated to the request structure.
If true, perform request for Address Info. If false, request the associated information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchNameInfo" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether to request Name Info associated to the request structure.
If true, perform request for Name Info. If false, request the associated information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="etfInfo" type="tns:EtfInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing ETF (Early Termination Fee) information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchPrivacyInfo" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the marketing privacy options list and cpni privacy options list for the BAN should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchEquipmentInfo" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the equipment info for the subscriber should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchServiceQuotaInfoList" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the Service Quota Information List should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchMarketingInfo" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the marketing information for this subscriber should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchPersonalizationAttributeInfo" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating whether or not the BANs personalized marketing attributes should be returned</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QuerySubscriberBasicInfoResponseType">
		<xs:sequence>
			<xs:element name="basicInfo" type="tns:BasicInfoType">
				<xs:annotation>
					<xs:documentation>Structure containing basic information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="detailInfo" type="tns:DetailInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing detailed general information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="etfInfo" type="tns:EtfInfoResponseType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing detailed ETF information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addressInfo" type="tns:AddressInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing detailed address information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nameInfo" type="tns:NameInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing detailed name information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="privacyInfo" type="tns:PrivacyListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing  detailed privacy information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="equipmentInfo" type="tns:EquipmentInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing  detailed privacy information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="serviceQuotaInfoList" type="tns:ServiceQuotaListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Only returned for CDMA subscribers.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="marketingInfo" type="tns:MarketingInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing marketing information for the subscriber
Note: Not returned on Real Time requests
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="personalizationAttributeInfo" type="tns:PersonalizationAttributeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>0 to 1 can be returned.  Will only return if the fetchPersonlizationAttributeInfo was set to tue.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressInfoType">
		<xs:sequence minOccurs="0">
			<xs:element name="type" type="tns:AddressTypeCodeType">
				<xs:annotation>
					<xs:documentation>Designates the address type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetNumber" type="tns:StreetNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Address Street Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="line1" type="tns:PrimaryAddressLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The content of Primary Address Line is dependent upon the type of street.  
For a ‘street address’, Primary Address Line holds the concatenation of the street components of the address.
For a ‘Post Office Box address’ (PO Box address), Primary Address Line holds the P.O. Box information.
For a ‘Rural address’, Primary Address Line holds the Rural number.
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="line2" type="tns:SecondaryAddressLineType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Secondary Address Line holds free text describing additional address information. Usually used for business customers.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ruralRoute" type="tns:RuralRouteControlNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rural Route</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ruralNumber" type="tns:RuralRouteBoxNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rural Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postOfficeBoxNumber" type="tns:PostOfficeBoxNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Post Of Office Box Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetName" type="tns:StreetNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Address Street Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetDirection" type="tns:PrecedingStreetDirectionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street Direction is a USPS Postal Address component that holds the compass direction that appears before the Street Name in some cases.  
For example in, "North Main Street", "North" would reside in Preceding Direction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="trailingDirection" type="tns:PrecedingStreetDirectionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Trailing Direction is the same as a Street Direction</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="streetType" type="tns:StreetSuffixType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Address Street Type is a suffix for USPS Postal Address component that holds the trailing part of the street name.  
For example in, "123 Main Street", "Street" would reside in Street Suffix.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartment" type="tns:ApartmentNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Apartment Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="apartmentType" type="tns:ApartmentDesignatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Apartment Type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attention" type="tns:AttentionTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Attention Text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="city" type="tns:CityNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>City Name holds the textual string identifying a City.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="state" type="tns:UspsStateAbbreviationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A United States Postal Service (USPS) State Abbreviation Code is the set of characters that are selected from a State or Protectorate name and specified by the USPS to represent the name of a State or US Protectorate for the purpose of mail delivery.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="tns:IsoCountryCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An International Standardization Organization (ISO) 3-Character Country Code is the set of three characters assigned by the ISO to identify a sovereign nation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode" type="tns:UspsPostalCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>USPS Postal Code contains the first 5 characters of the nine digits USPS Postal Code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zip4Code" type="tns:UspsPostalCodeExtensionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>USPS Postal Code Extension contains the last 4 characters of the nine digits USPS Postal Code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geoCode" type="tns:GeographicBoundaryIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Geographical Code holds a value that uniquely identifies an instance of an object within the context of a System, Database or a Model.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="effectiveDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the address was recorded.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isGeoCodeOverridden" type="tns:GeographicCodeOverrideFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>‘true’ if ADDRESS_DATA.ORIG_ADR_ZIP_GEO_CODE != ADDRESS_DATA.ADR_ZIP_GEO_CODE</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BasicInfoType">
		<xs:sequence>
			<xs:element name="ban" type="tns:BillingAccountNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing Account Number uniquely identifies an instance of Billing Account used to accrue charges for invoicing.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberId" type="tns:SubscriptionIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Subscriber Identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alias" type="tns:SubscriberNameStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the subscriber name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="tns:SubscriberStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the status of a subscriber.
A – Active
S – Suspended
C – Canceled
R – Reserved
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusChangeDate" type="tns:LastUpdateDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the latest status changed.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusChangeReasonCode" type="tns:SubscriberStatusChangeReasonCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates change reason code of subscriber status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="activationDate" type="tns:SubscriberActivationDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar date on which the subscriber becomes active in the system.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lastActivityCode" type="tns:SubscriberActivityCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies the last activity preformed on a subscriber.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineType" type="tns:SubscriptionRelationshipTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies whether this value represents an Alternate Line (A) or Split Bill (S) subscriber.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="personalBan" type="tns:BillingAccountNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing Account Number Id of the personal BAN.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="networkInd" type="tns:NetworkTypeCodeType" block="restriction" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identfies the subscribrs network. C for CDMA and I for iDEN</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hotlineInd" type="tns:CustomerServiceHotlineQueuedFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates whether a customer level is hotline or not. ‘true’ for hotlined and ‘false’ for not hotlined.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hotlineReasonCode" type="tns:HotlineReasonCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies the Hotline SOC attached to the subscriber.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="legacySubscriberId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Legacy Subscription Identifier of the customer. This is returned for converted subscriber only.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:SubscriptionIdentifierType">
						<xs:minLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="subscriberPrepayInd" type="tns:SubscriberPrePayTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SUBSCRIBER.SUB_PREPAID_IND</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mdnPendingPortOutInd" type="tns:PendingPortOutFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates a boolean indicator of whether the subscriber's MDN port out is pending.
If true, Port out is pending. If false, Port out is not pending</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tclHotlineInd" type="tns:CustomerServiceHotlineQueuedFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates whether the subscriber is TCL hotlined.
If true, is TCL hotlined. If false, is not TCL hotlined.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountType" type="tns:BillingAccountTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing Account Type Code categorizes BILLING_ACCOUNT instances into pre-defined segments used primarily for Customer segmentation in General Ledger reporting and Collection treatment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountSubType" type="tns:BillingAccountSubTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Billing Account SubType Code further categorizes the Billing Account within the context of the assigned Billing Account Type Code used primarily for Customer segmentation in General Ledger reporting and collection treatment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountStatus" type="tns:BillingAccountStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Valid states:
O - Open (active) 
C - Cancelled
S - Suspended
T - Tentative
N -  Closed
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountTypeDesc" type="tns:BillingAccountTypeDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Account Type description text (i.e Individual)
It is specific to IDEN Devices Only
Not returned for Ensemble Wholesale customers</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="premiumInd" type="tns:BinaryExpressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies Premium Price Plan</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BusinessNameInfoType">
		<xs:sequence>
			<xs:element name="businessName" type="tns:BusinessNameType">
				<xs:annotation>
					<xs:documentation>Business Name is a label applied to those instances of ORGANIZATION of type BUSINESS.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalLine" type="tns:BusinessNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Additional Line designates additional business name information</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CpniPrivacyListType">
		<xs:sequence>
			<xs:element name="cpniPrivacyInfo" type="tns:PrivacyInfoType" maxOccurs="20"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DetailInfoType">
		<xs:sequence minOccurs="0">
			<xs:element name="mdn" type="tns:MobileDirectoryNumberStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Mobile Directory Number is the method of identifying telephone trunks in the public network of North America called World Numbering Zone 1 by the ITU-T. the Plan has 3 ways of identifying phone numbers in North America -- a 3-digit area code, a 3-digit exchange or central office code, and 4-digit subscriber code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mdnStatus" type="tns:SubscriberServiceStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>if SUBSCRIBER_RSOURCE.PTN_STATUS = 'C' and PTN_INV.RESOURCE_STATUS = 'AG' set ptsStatus = 'G' else return PTN_STATUS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mdnStatusDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The last time MDN was updated.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ufmi" type="tns:UrbanFleetMemberIdentifierStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The subscriber’s UFMI - Urban Fleet Member Identifier String contains the concatenation of the Urban Identifier,'*' Fleet Identifier, '*' and Member Identifier used to identify the Direct Connect Subscriber in the iDen network.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ufmiStatus" type="tns:SubscriberServiceStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>if SUBSCRIBER_RSOURCE.UFMI_STATUS = 'C' and UFMI_INV.RESOURCE_STATUS = 'AG' set ufmiStatus = 'G' else return UFMI_STATUS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ufmiStatusDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the latest status was effected.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ufmiRangeInd" type="tns:UfmiOutOfRangeFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates whether the UFMI is out of range.
If ‘true’, UFMI is out of range. If ‘false’, UFMI is not out of range.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ipAddress" type="tns:InternetProtocolAddressStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The static Internet Protocol Address of the subscriber </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ipStatus" type="tns:SubscriberServiceStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>if SUBSCRIBER_RSOURCE.IP_STATUS = 'C' and IP_INV.RESOURCE_STATUS = 'AG' set ipStatus = 'G' else return IP_STATUS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ipStatusDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the latest IP status occurred.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ipNgp" type="tns:StaticInternetProtocolLocationDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Internet Protocol Number Group code for Static IP address allocation (for the CDMA Static Public IP resource data feature).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ipNgpDesc" type="tns:StaticInternetProtocolLocationDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates ipNgp description</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="imsi" type="tns:InternationalMobileSubscriberIdentityStringType">
				<xs:annotation>
					<xs:documentation>For CDMA subscriber this field contains the value of MSID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalImsi" type="tns:InternationalMobileSubscriberIdentityStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Additional International Mobile Subscriber Identity, for dual Phones. 
To be populated for CDMA subscribers.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="portInMdn" type="tns:MobileDirectoryNumberStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the port in PTN, if the PTN is a temporary PTN and a port in request is pending.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="imei" type="tns:InternationalMobileEquipmentIdentifierStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>International Mobile Equipment Identity of the subscriber.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sim" type="tns:SubscriberIdentityModuleIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Subscriber Identify Module (SIM) Identifier identifies the ''smart'' card installed or inserted into a IDEN mobile telephone containing all subscriber-related data. This facilitates a telephone call from any valid mobile telephone since the subscriber data is used to complete the call rather than the telephone internal serial number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="msn" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For CDMA subscriber this field contains the value of ESN</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:ManufacturerSerialNumberType">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="nai" type="tns:NetworkAccessIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the subscriber’s primary NAI.
Network Access Identifier (NAI) (e.g. username@domain) where domain should be of the form str1.str2. No validation will be done on input value.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="naiStatus" type="tns:SubscriberServiceStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the status of a subscriber's primary NAI.
Valid values: A for Active, C for Canceled, G for Aging, R for Reserved, S for Suspended.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="naiStatusDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the latest primary NAI status occurred.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="naiStatusChangeReason" type="tns:SubscriberStatusChangeReasonDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the reason why NAI was enabled or disabled.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="naiTemporaryDisabledInd" type="tns:NetworkAccessIdentifierTemporaryDisableFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates whether an NAI is temporarily disabled.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="esn" type="tns:SubscriberDeviceSerialNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates subscriber’s ENS.
Electronic Serial Number (ENS) identifies a device of any subscriber under the BAN.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="esnHex" type="tns:SubscriberDeviceSerialNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CDMA Subscribers’ ESN/MEID in hexadecimal format.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="macId" type="tns:MediumAccessControlAddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique Media Access Control Address identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberAttributeCode" type="tns:AiraveServiceTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Subscriber special attribute defining the type of Airave or 4G service the subscriber has.
FEM_ARV
FEMARV2
ATA_ARV
4G_IND</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="iccId" type="tns:UniversalIntegratedCircuitCardIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Integrated Circuit Card Identifier of the LTE removable UICC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricePlanCode" type="tns:ServiceOrderCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the subscriber’s price plan.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dealerCode" type="tns:DealerIdentifierType">
				<xs:annotation>
					<xs:documentation>Designates Dealer Code, which is the agent or dealer who handled the customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subMarket" type="tns:SubMarketCodeType">
				<xs:annotation>
					<xs:documentation>Designates the subscriber’s submarket code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultDealerCode" type="tns:DealerIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>select DEALER_CODE from EC_DEFAULT_DEALER   

where SUB_MARKET= SUBSCRIBER.SUB_MARKET and HIERARCHY_SOURCE = 'NCI';
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="csa" type="tns:CommunicationServiceAreaIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Communication Service Area Identifier uniquely identifies an instance of a geographic area (CSA) served by a communication service provider, which can be broken down into regional, metropolitan, district and smaller sections called ''leaves.''</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="csaDesc" type="tns:CommunicationServiceAreaNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Communication Service Area Name the name of a geographic area (CSA) served by a communication service provider, which can be broken down into regional, metropolitan, district and smaller sections called ''leaves.''</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="affiliateSubscriberCsaInd" type="tns:AffiliateTerritoryFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates whether the subscriber level CSA is in affiliate territory or not. 
If true, the subscriber level CSA is in Affiliate territory. If false, it is not in Affiliate territory</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hca" type="tns:HomeCallingAreaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>"Home Calling Area" is where all of the calls made on a wireless device are free.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="location" type="tns:NumberLocationCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The location where the subscriber was activated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="committedMonths" type="tns:CommitmentMonthCountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the number of commitment months.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commitmentStartDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the commitment started.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commitmentEndDate" type="tns:EffectiveThruDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the commitment ended, based on subscriber’s price plan.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="etf" type="tns:ChargeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Early Termination Fee represents a dollar amount billed to the Customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="prepayInd" type="tns:PrepayTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Prepay Indicator designates a specific prepay type.
Valid values: REGLR (regular), BOOST (Boost), SIMPR (SIM Prepaid).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mdnType" type="tns:PortedNumberIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the Mobile Dialing Number (MDN) Type.
Valid values: RGL for Regular, PRI for Port In, PRO for Port Out, IPI for Regular</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="portOutInd" type="tns:NumberGuardFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Port Out Indicator determines whether or not there is a number guard (port out freeze) on this PTN.
If true, the PTN has a number guard. If false, the PTN does not have a number guard.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="communicationMethod" type="tns:ContactMethodTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the communication method that may be used to contact the subscriber.
Valid values: E for Email, F for Fax</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="communicationEmail" type="tns:EmailAddressStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates an email Address on which the customer can be contacted.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="communicationFax" type="tns:MobileDirectoryNumberStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates fax number which the customer can be contacted.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="securityLockCode" type="tns:PhoneLockCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customers use the Security Lock Code to unlock their handset, for CDMA phones.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="portStatus" type="tns:PortInStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the subscriber’s port in status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusText" type="tns:PortInStatusDescriptionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the port status description.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractUpdateType" type="tns:ContractUpdateTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contract Method Type Code uniquely identifies a method that may be used to contact a subscription.
Valid Values: N for New Contract, U for Update Contract after RPPP, R for Update Contract during RPPP, C for Cancel Contract after RPPP, P for Cancel Contract during RPPP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractDealerCode" type="tns:DealerIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the Dealer Code of the contract, which is the agent or dealer who handled the customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractEffectiveDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the contract was effected.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="socEtf" type="tns:ChargeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>PP_OC_RATE.RATE where SOC =  ‘DEFNXT’ and FEATURE_CODE = ‘PNETRM’.  socEarlyTermintaionFee  should be populated if commitEndDate is in future.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hppttId" type="tns:HighPerformancePushToTalkIdentifierStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>High Performance Push-To-Talk Identifier contains the concatenation of a user ID and a domain. The user ID can be 15 chars maximum and the domain can be 24 chars maximum so a total of 40 characters including @.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hppttType" type="tns:HighPerformancePushToTalkResourceIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>P - CDMA PTN
C - CDMA UFMI
I - iDEN UFMI</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hppttStatus" type="tns:SubscriberStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>(R)eserved, (A)ctive, (S)uspended, (C)anceled</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="promoDealer" type="tns:DealerIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the Dealer who initiated the subscriber.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="callerIdName" type="tns:CallerIdNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A caller’s name that appears in the equipment screen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="oArssDate" type="tns:EffectiveFromDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the calendar day on which the subscriber should be programmed via ORSS.
Format: YYYY-MM-DD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="timezone" type="tns:TimeZoneStringType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Applies only to prepaid subs</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="svmNumber" type="tns:FeatureSpecialPhoneNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Designates the Single Voicemail (SVM) Numbers for the associated SVM feature.
Note: SVM number is also known as MFN number.
This field will be populated if and only if the svmInd field below is set to true.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="svmInd" type="tns:SingleVoicemailFlagType">
				<xs:annotation>
					<xs:documentation>Applies only for SVM SOC Set  the Flag 'true' when AGREEMENT_ATTR table ATTR_CODE='IO_SVM' otherwise set to 'false'</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="splitBillInd" type="tns:SubscriberSplitBillCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Subscriber Split Bill Code is an indicator of a split Bill Subscriber.
Valid values: P for Primary subscriber on the split bill, S for Secondary subscriber on the split bill.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="npaSplit" type="tns:NpaNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the New NPA after a split had happened.
Numbering Plan Area (NPA) number is one of the geographical divisions of the United States, Canada, Bermuda, the Caribbean, Northwestern Mexico, Alaska, Hawaii, within which no two telephones will have the same seven-digit telephone number. (An NPA is represented by an Area Code.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberOrderCommittedCode" type="tns:OrderCommittedCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Subscriber Order Committed Code indicates how the subscriber is committed to an order.
Valid values: Y for Order Completed, ‘C’ for Committed to Order, F for Failed in Activation, N for Not Committed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adminSmsReceivableInd" type="tns:AdminSmsReceiveCapableFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Admin SMS Receive Capable Indicator determines if the subscriber is capable of receiving SMS from an admin account.
If true, is capable. If false, is not capable.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="itemId" type="tns:ModelNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Item Identifier
This field will be returned based on the customer handset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ufpEligibleInd" type="tns:EquipmentUpgradeIndicatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Will only be returned if subscriber is a UFP(upgrade frequency period) subscriber

If current date > SUSBCRIBER_UFP.UFP_END date then 'true'. else false
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ufpEligibleDate" type="tns:UpgradeEligibilityDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Will only be returned if subscriber is a UFP subscriber
SUBSCRIBER_UFP.UFP_END date

</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="upSubscriberId" type="tns:UpSubscriberGatewayIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the UP Subscriber Gateway Identifier, and is only available to iDEN subscribers.  Will only be returned if it was the search criteria on the input.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="languageCode" type="tns:LanguageTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Language Preference Code identifies a language preference.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="universalInd" type="tns:BinaryExpressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the subscriber’s price plan is a Universal price plan
Valid values:
true – Universal price plan
false – Non-Universal price plan
Note: Only returned on Real Time Requests</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="offerMapLevel" type="tns:OfferMapLevelType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the subscriber’s price plan has Enabler features that are mapped to Enabler offers. This indicates whether Enabler features can be excluded from or included in the price plan.
Valid values:
S – Price plan itself is mapped to Enabler offer (Enabler features cannot be excluded from or included in the price plan)
F – Price plan’s Enabler features are  mapped to Enabler offers (Enabler features can be excluded from or included in the price plan)
Note: Only returned on Real Time Requests</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="relationInd" type="tns:PrimarySecondaryTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>P – Primary bundle 
S – Secondary bundle 
B – Both bundles
N – No bundle
Note: Only returned on Real Time Requests</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minSequenceNumber" type="tns:SequenceNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The subscriber’s minimum sequence number from SUB_RES_HISTORY
Note: Only returned on Real Time Requests</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataLinkInd" type="tns:BinaryExpressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the subscriber has a current-dated or future-dated Data Link SOC.
Valid values are:
true– Subscriber has current or future-dated Data Link SOC
false – Subscriber does not have a current or future-dated Data Link SOC
Note: Only returned on Real Time Requests</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EquipmentInfoType">
		<xs:sequence>
			<xs:element name="itemId" type="tns:ModelNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An ID which uniquely identifies a device model</xs:documentation>
				</xs:annotation>
			</xs:element>
