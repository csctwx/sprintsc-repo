	<xs:element name="querySubscriberServices" type="tns:QuerySubscriberServicesRequest"/>
	<xs:element name="querySubscriberServicesResponse" type="tns:QuerySubscriberServicesResponse"/>
	<xs:complexType name="QuerySubscriberServicesRequest">
		<xs:all>
			<xs:element name="info">
				<xs:complexType>
					<xs:choice>
						<xs:element name="banId" type="CustomerAccountNumber"/>
						<xs:element name="subsId" type="SubscriptionId"/>
						<xs:element name="ptn" type="MdnString"/>
						<xs:element name="equipmentId" type="DeviceSerialNbr"/>
						<xs:element name="ufmi" type="UfmiString"/>
						<xs:element name="ip" type="ElectronicAddressString"/>
						<xs:element name="nai" type="NetworkAccessIdentifierString"/>
						<xs:element name="imsi" type="InternationalMobileSubscriberIdentityString"/>
						<xs:element name="hppttId" type="HighPerformancePushToTalkIdString"/>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="featureList" type="tns:FetchFeatureListType" minOccurs="0"/>
			<xs:element name="ltsList" type="tns:FetchInformationFlagType" minOccurs="0"/>
			<xs:element name="m2HONumberInfo" type="tns:FetchM2HONumberInfoType" minOccurs="0"/>
			<xs:element name="socList" type="tns:FetchSocListType" minOccurs="0"/>
			<xs:element name="hybridGroupList" type="tns:FetchHybridGroupListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether response should include the hybridGroupList, which contains info of the hybrid sharing groups under the ban to which the input subscriber belongs</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fetchSubAttributeInfo" type="tns:FetchInformationFlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether response should inclue subscriberAttributeInfo</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="QuerySubscriberServicesResponse">
		<xs:all>
			<xs:element name="pricePlanInfo" type="tns:PricePlanInfoType"/>
			<xs:element name="featureList" type="tns:FeatureListType" minOccurs="0"/>
			<xs:element name="ltsList" type="tns:LtsListType" minOccurs="0"/>
			<xs:element name="m2HONumberInfo" type="tns:M2HONumberInfoType" minOccurs="0"/>
			<xs:element name="socList" type="tns:SocListType" minOccurs="0"/>
			<xs:element name="hybridGroupList" type="tns:HybridGroupListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>hybridGroup Info structure
contains the hybrid sharing group info under the ban to which the input subscriber belongs</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscriberAttributesInfo" type="tns:SubscriberAttributeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure containing subscriber information</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="AllowanceDataInfoType">
		<xs:sequence>
			<xs:element name="allowanceType" type="tns:AllowanceTypeCodeType">
				<xs:annotation>
					<xs:documentation>Type of allowance
Sprint 3G Data
Sprint 4G Data
Sprint 3G/4G Data</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="allowance" type="tns:UnitOfMeasureValueType">
				<xs:annotation>
					<xs:documentation>Includes units of allowance for the input subscriber in MB</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AllowanceDataListType">
		<xs:sequence>
			<xs:element name="allowanceDataInfo" type="tns:AllowanceDataInfoType" maxOccurs="3"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AllowanceInfoType">
		<xs:sequence>
			<xs:element name="allowanceType" type="tns:AllowanceTypeCodeType">
				<xs:annotation>
					<xs:documentation>Type of allowance
Sprint Data
Sprint Data Roaming</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subsAllowance" type="tns:UnitOfMeasureValueType">
				<xs:annotation>
					<xs:documentation>Included units of allowance for the input subscriber in MB</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currentGroupAllowance" type="tns:UnitOfMeasureValueType">
				<xs:annotation>
					<xs:documentation>Current Hybrid Group Allowance in MB</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nextCycleGroupAllowance" type="tns:UnitOfMeasureValueType">
				<xs:annotation>
					<xs:documentation>Next Cycle Hybrid Group Allowance in MB</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AllowanceListType">
		<xs:sequence>
			<xs:element name="allowanceInfo" type="tns:AllowanceInfoType" maxOccurs="10"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeatureListType">
		<xs:sequence minOccurs="0">
			<xs:element name="featureInfo" type="tns:FeatureInfoType" minOccurs="0" maxOccurs="50"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeatureInfoType">
		<xs:sequence>
			<xs:element name="socCode" type="tns:SocCode"/>
			<xs:element name="socType" type="tns:SocType"/>
			<xs:element name="featureCode" type="tns:FeatureCode"/>
			<xs:element name="serviceType">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name" type="tns:FeatureDescription" minOccurs="0"/>
			<xs:element name="effectiveDate" type="xs:date"/>
			<xs:element name="expiryDate" type="xs:date" minOccurs="0"/>
			<xs:element name="msisdn" type="tns:MdnString" minOccurs="0"/>
			<xs:element name="freeMinType" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="isUsageType" type="xs:boolean" minOccurs="0"/>
			<xs:element name="isSwitchType" type="xs:boolean" minOccurs="0"/>
			<xs:element name="socSeqNumber" type="tns:SocSequenceNumber" minOccurs="0"/>
			<xs:element name="featureSeqNumber" type="tns:FeatureSequenceNumber" minOccurs="0"/>
			<xs:element name="m2hoType" block="restriction" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="pttInd" type="xs:boolean" block="restriction" minOccurs="0"/>
			<xs:element name="pttNaiReq" type="xs:boolean" block="restriction" minOccurs="0"/>
			<xs:element name="pamInd" type="xs:boolean" block="restriction"/>
			<xs:element name="networkInd" type="tns:NetworkIndicatorCode" block="restriction"/>
			<xs:element name="naiPtt" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="naiPam" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="hppttInd" type="xs:boolean"/>
			<xs:element name="spanishFeatureDesc" type="tns:FeatureDescription" minOccurs="0"/>
			<xs:element name="evdoInd" type="xs:boolean"/>
			<xs:element name="evdoRev" minOccurs="0">
				<xs:annotation>
					<xs:documentation>REV0
REVA</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="abInd" type="xs:boolean"/>
			<xs:element name="glmsInd" type="xs:boolean"/>
			<xs:element name="winInd" type="xs:boolean"/>
			<xs:element name="hppttId" type="HighPerformancePushToTalkIdString" minOccurs="0"/>
			<xs:element name="hppttType" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="recurringCharge" type="xs:decimal" minOccurs="0"/>
			<xs:element name="billDisplayInd" type="tns:BinaryExpressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to determine if the feature should be presented on customer's invoice.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="meterCode" type="tns:MeterTypeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Meter SOC indicator
M= metered
U= unmetered 
Subscriber with a suspend as a prevention for bill shock by giving the subscriber the alert with an option to move into overages.

</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FetchFeatureListType">
		<xs:simpleContent>
			<xs:extension base="xs:boolean">
				<xs:attribute name="includeFutureDated" type="tns:FetchInformationFlagType" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="FetchHybridGroupListType">
		<xs:sequence>
			<xs:element name="fetchHybridGroupList" type="tns:FetchInformationFlagType"/>
			<xs:element name="nextDate" type="tns:NextDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Must be in the future

When sent will retrieve the nextDateSubsCount till this date

Default behavior when not sent is to get the nextDateSubsCount at the start of the nextBill Cycle</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FetchM2HONumberInfoType">
		<xs:simpleContent>
			<xs:extension base="xs:boolean">
				<xs:attribute name="includeExpired" type="tns:FetchInformationFlagType" use="optional"/>
				<xs:attribute name="includeFutureDated" type="tns:FetchInformationFlagType" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="HybridGroupInfoType">
		<xs:sequence>
			<xs:element name="sharingGroupId" type="tns:ServiceOrderCodeType">
				<xs:annotation>
					<xs:documentation>The Hybrid Group Sharing Plan Id. Id that defines one hybrid group</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maxSubsAllowed" type="tns:MaximumSubscriberCountType">
				<xs:annotation>
					<xs:documentation>Maximum subscribers allowed under a Hybrid group</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currentSubsCount" type="tns:SubscriberCountType">
				<xs:annotation>
					<xs:documentation>Count of current number of subscribers under the Hybrid group in current Bill Cycle</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nextDateSubsCount" type="tns:SubscriberCountType">
				<xs:annotation>
					<xs:documentation>Count of number of subscribers belonging to the Hybrid Group till the nextDate if provided in the input. If not provided it gives the subscriber count as start of Next bill cycle</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="allowanceList" type="tns:AllowanceListType">
				<xs:annotation>
					<xs:documentation>List of allowanceInfo structure
AllowanceList will contain allowance information for the hybrid sharing group soc of the subscriber.
For Data, two allowanceInfo structures can be sent, one for “Sprint Data” and second for “Sprint Data Roaming” depending upon the soc setup.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HybridGroupListType">
		<xs:sequence>
			<xs:element name="hybridGroupInfo" type="tns:HybridGroupInfoType" maxOccurs="50"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LtsInfoType">
		<xs:sequence>
			<xs:element name="groupType" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="soc" type="tns:SocCode"/>
			<xs:element name="socDescription" type="tns:SocDescription" minOccurs="0"/>
			<xs:element name="socType" type="tns:SocType"/>
			<xs:element name="sharingGroupId" type="tns:LinesToShareGroupIdentifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The LTS data sharing group identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maxSubsAllowed" type="xs:integer"/>
			<xs:element name="currentRecurringCharge" type="tns:RecurringCharge"/>
			<xs:element name="currentRank" type="xs:integer"/>
			<xs:element name="rankOverriddenFlag" type="xs:boolean" minOccurs="0"/>
			<xs:element name="serviceEffectiveDate" type="xs:date"/>
			<xs:element name="subscriberList" type="tns:SubscriberListType"/>
			<xs:element name="universalInd" type="tns:BinaryExpressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Will indicate whether the price plan/SOC is a Universal price plan
Valid values:
true – Universal price plan/SOC
False or not returned – Non-Universal price plan/SOC
Note: Returned only on Real Time Requests</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="offerMapLevel" type="tns:OfferMapLevelType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicate whether the price plan/SOC has Enabler features that are mapped to Enabler offers.
Valid values:
S – Price plan/SOC itself is mapped to Enabler offer 
F – Price plan/SOC’s Enabler features are mapped to Enabler offers 
N  – Price plan/SOC is not mapped to an Enabler offer
Note: Returned only on Real Time Requests</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LtsListType">
		<xs:sequence>
			<xs:element name="ltsInfo" type="tns:LtsInfoType" minOccurs="0" maxOccurs="50"/>
			<xs:element name="nextBillCycleLtsInfo" minOccurs="0" maxOccurs="50">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="groupType" type="xs:string" minOccurs="0"/>
						<xs:element name="soc" type="tns:SocCode"/>
						<xs:element name="socDescription" type="tns:SocDescription" minOccurs="0"/>
						<xs:element name="socType" type="tns:SocType"/>
						<xs:element name="sharingGroupId" type="tns:LinesToShareGroupIdentifierType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The LTS data sharing group identifier.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="maxSubsAllowed" type="xs:integer"/>
						<xs:element name="recurringCharge" type="tns:RecurringCharge"/>
						<xs:element name="rank" type="xs:integer"/>
						<xs:element name="rankOverriddenFlag" type="xs:boolean" minOccurs="0"/>
						<xs:element name="serviceEffectiveDate" type="xs:date"/>
						<xs:element name="subscriberList">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="subscriberInfo" maxOccurs="50">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="subsId" type="tns:SubscriptionId">
													<xs:annotation>
														<xs:documentation>SERVICE_AGREEMENT.SUBSCRIBER_NO</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ptn" type="tns:MdnString">
													<xs:annotation>
														<xs:documentation>SUBSCRIBER_RSOURCE.PTN</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="recurringCharge" type="tns:RecurringCharge">
													<xs:annotation>
														<xs:documentation>SOC_RC_RANK.RC</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="rank" type="xs:integer">
													<xs:annotation>
														<xs:documentation>Rank should be calculated based on the Effectiv Date and Feature Seq Number</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="serviceEffectiveDate" type="xs:date">
													<xs:annotation>
														<xs:documentation>SERVICE_FEATURE.FTR_EFFECTIVE_DATE</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="rankOverriddenFlag" type="xs:boolean" minOccurs="0">
													<xs:annotation>
														<xs:documentation>'True' if SERVICE_FEATURE.RANK  is not null else 'False'</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="minSeqNumber" type="tns:SequenceNumberType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>The subscriber’s minimum sequence number from SUB_RES_HISTORY</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="initialActivationDate" type="tns:InitialSubscriptionActivationDateType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Initial activation date of the subscriber (for reserved subscribers with no future activation this date will be populated with the creation date).</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="universalInd" type="tns:BinaryExpressionType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Will indicate whether the price plan/SOC is a Universal price plan
Valid values:
true – Universal price plan/SOC
False or not returned – Non-Universal price plan/SOC
Note: Returned only on Real Time Requests</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="offerMapLevel" type="tns:OfferMapLevelType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Indicate whether the price plan/SOC has Enabler features that are mapped to Enabler offers.
Valid values:
S – Price plan/SOC itself is mapped to Enabler offer 
F – Price plan/SOC’s Enabler features are mapped to Enabler offers 
N  – Price plan/SOC is not mapped to an Enabler offer
Note: Returned only on Real Time Requests</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
