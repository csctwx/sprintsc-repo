							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="jvVmSocInd" type="tns:BinaryExpressionType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Calculated From AGREEMENT_ATTR table
Note: Returned only on Real Time requests</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="bundle" type="tns:PrimarySecondaryTypeCodeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>SOC.BUNDLE : P - Primary bundled PP/SOC, S - Secondary bundled PP/SOC, no tag for none</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="universalInd" type="tns:BinaryExpressionType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>UPP/SOC indicator (true/false)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="offerMapLevel" type="tns:OfferMapLevelType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Enabler features level (S/F/" ")</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="blockAllowListSocInd" type="tns:BinaryExpressionType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Indicator whether the soc is  Block/Allow PTN  Listl(true/false)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="prepaidSocAttributeInfo" type="tns:PrepaidSocAttributeInfoType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Structure containing attributes of the SOC</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="discountAmt" type="tns:DiscountAmountType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The discount amount due to the spotify SOC on the subscription</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubscriberAttributeInfoType">
		<xs:sequence>
			<xs:element name="vowifi911ConsentFlag" type="tns:Consent911FlagType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indicating if subscriber has given 911 consent</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minSeqNumber" type="tns:SequenceNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The subscriber’s minimum sequence number from SUB_RES_HISTORY</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="initialActivationDate" type="tns:InitialSubscriptionActivationDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Initial activation date of the subscriber (for reserved subscribers with no future activation this date will be populated with the creation date)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubscriberInfoType">
		<xs:sequence>
			<xs:element name="subsId" type="tns:SubscriptionId"/>
			<xs:element name="ptn" type="tns:MdnString"/>
			<xs:element name="currentRecurringCharge" type="tns:RecurringCharge"/>
			<xs:element name="currentRank" type="xs:integer"/>
			<xs:element name="serviceEffectiveDate" type="xs:date"/>
			<xs:element name="rankOverriddenFlag" type="xs:boolean" minOccurs="0"/>
			<xs:element name="minSeqNumber" type="tns:SequenceNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The subscriber’s minimum sequence number from SUB_RES_HISTORY</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="initialActivationDate" type="tns:InitialSubscriptionActivationDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Initial activation date of the subscriber (for reserved subscribers with no future activation this date will be populated with the creation date).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubscriberListType">
		<xs:sequence>
			<xs:element name="subscriberInfo" type="tns:SubscriberInfoType" maxOccurs="50"/>
		</xs:sequence>
	</xs:complexType>
	<!---->
	<!--     MVM Simple Types/Shared Simple Types-->
	<xs:simpleType name="AllowanceTypeCodeType">
		<xs:annotation>
			<xs:documentation>Allowance Type Code designates a type of allowance of units that would be set up for a user, espically in the case of a hybrid sharing group.
Valid Values Include:
Sprint Data,
Sprint Data Roaming...</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AnniversaryDateType">
		<xs:annotation>
			<xs:documentation>Anniversary Date is the subscriber anniversary date for RC plan standard date format (GMT) yyyy-MM-dd</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="BinaryExpressionType">
		<xs:annotation>
			<xs:documentation>Binary Expression is a type of an expression with two possible states,"true"and"false".</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<xs:simpleType name="Consent911FlagType">
		<xs:annotation>
			<xs:documentation>Consent 911 Flag is a binary indicator of whetther the subscriber has given 911 consent.
Valid Values:
true - the subcriber has given 911 consent
false - the subscriber has not given 911 consent</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<xs:simpleType name="DeviceCategoryIdentifierType">
		<xs:annotation>
			<xs:documentation>Device Category Identifier uniquley identifies a category of the deivce.
Valid Values Include:
smart phone,
basic phone,
usb,
mbc,
tablet,
mobilehotspothandset,
netbook,
data card,
tablet,
mifi/overdrive...</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DiscountAmountType">
		<xs:annotation>
			<xs:documentation>Discount Amount holds the monetary amount by which the the price charged for a GOOD or SERVICE is decreased.  A percent decrease in the price charged may only be applicable to specific Account Types, Promotions, Products, etc.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="FetchInformationFlagType">
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<xs:simpleType name="InitialSubscriptionActivationDateType">
		<xs:annotation>
			<xs:documentation>Initial Subscription Activation Date is the calendar month, day and year of the Subscriptions first network activation.  This date never changes.  Initial Subscription Activation Date provides the capability of calculating the total duration of the Subscription's existence regardless of interim deactivation activation events.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="LinesToShareGroupIdentifierType">
		<xs:annotation>
			<xs:documentation>Lines To Share Group Identifier uniquely identifies a Lines To Share Group within a Billing Account.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer"/>
	</xs:simpleType>
	<xs:simpleType name="MaximumSubscriberCountType">
		<xs:annotation>
			<xs:documentation>Maximum Subscriber Count is the maximum number of subscribers for the associated account.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int"/>
	</xs:simpleType>
	<xs:simpleType name="MeterTypeCodeType">
		<xs:annotation>
			<xs:documentation>Meter Type Code indicates whether the associated product is metered.
Valid Values:
M= metered,
U= unmetered </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="M"/>
			<xs:enumeration value="U"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MobileDirectoryNumberStringType">
		<xs:annotation>
			<xs:documentation>The method of identifying telephone trunks in the public network of North America, called World Numbering Zone 1 by the ITU-T. the Plan has 3 ways of identifying phone numbers in North America -- a 3-digit area code, a 3-digit exchange or central office code, and 4-digit subscriber code.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NetworkAccessActivationFromDateType">
		<xs:annotation>
			<xs:documentation>Network Access Activiated From Date holds the first calendar day the set of implemented components were active on the network.

Elements declared to be of type date must follow this form:  
CCYY-MM-DD
range for CC is: 00-99
range for YY is: 00-99
range for MM is: 01-12
range for DD is: 
01-28 if month is 2
01-29 if month is 2 and the gYear is a leap gYear
01-30 if month is 4, 6, 9, or 11
01-31 if month is 1, 3, 5, 7, 8, 10, or 12

Example: 1999-05-31 represents May 31, 1999</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="NextDate">
		<xs:annotation>
			<xs:documentation>Deprecate - Use EffectiveThruDate</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="OfferMapLevelType">
		<xs:annotation>
			<xs:documentation>Offer Map level whether the price plan/SOC has Enabler features that are mapped to Enabler offers. This indicates whether Enabler features can be excluded from or included in the price plan/SOC.
Valid values:
S – Price plan/SOC itself is mapped to Enabler offer (Enabler features cannot be excluded from or included in the price plan/SOC)
F – Price plan/SOC’s Enabler features are mapped to Enabler offers (Enabler features can be excluded from or included in the price plan/SOC)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PrimarySecondaryTypeCodeType">
		<xs:annotation>
			<xs:documentation>Primary Secondary Type Code Type indicates whether the associated item is primary or secondary.
Valid values:
P - Primary,
S - Secondary,
B - Both...</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="P"/>
			<xs:enumeration value="S"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RecurringChargeAmountType">
		<xs:annotation>
			<xs:documentation>Recurring Charge Amount (RC) is a monthly fee charged for granting access to certain services. The RC is charged for subscribers who have purchased the offer for an unlimited time. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="ServiceCategoryNameType">
		<xs:annotation>
			<xs:documentation>Service Cateogory Name is the common name corresponding to the Usage Category Code.
C - Cellular Voice
S - SMS
D - Dispatch
V - Vision Data
P - 2G Data</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="25"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SequenceNumberType">
		<xs:annotation>
			<xs:documentation>Sequence Number is a unique positive integer assigned to an item in a list of items.  It is used to determine order in a list of items or to identify a specific item.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="13"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ServiceOrderCodeType">
		<xs:annotation>
			<xs:documentation>Service Order Code - Ensemble</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ServiceOrderCodeTypeCodeType">
		<xs:annotation>
			<xs:documentation>Service Order Code Type Code identifies the various types of Service Order Codes.  Valid values include (not exclusive list):
B - Buy One Get One SOC
I - 
P - Price Plan
M - Reduced Price Plan
S - Reduced SOC
R - Regular SOC
O - Optional SOC</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ServiceOrderDescriptionType">
		<xs:annotation>
			<xs:documentation>Service Order Description is a description of the product defined by a service order code.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SpecialServiceOrderCodeTypeCodeType">
		<xs:annotation>
			<xs:documentation>Special Service Order Code Type Code indicates how the given SOC is identified as a Special SOC
Valid Values:
L – Loyalty, 
V – VIP,
N - None</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="L"/>
			<xs:enumeration value="V"/>
			<xs:enumeration value="N"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SubscriberCountType">
		<xs:annotation>
			<xs:documentation>Subscriber Count is the number of subscribers associated to the BAN.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int"/>
	</xs:simpleType>
	<xs:simpleType name="SubscriberStatusCodeType">
		<xs:annotation>
			<xs:documentation>Subscriber status code is a single character code that represents the status of a subsciber.&#13;
&#13;
Possible values&#13;
A - Active&#13;
S - Suspended&#13;
C - Canceled&#13;
R - Reserved</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="S"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SubscriptionIdentifierType">
		<xs:annotation>
			<xs:documentation>Subscription Identifier uniquely identifies an instance of SUBSCRIPTION.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="8"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UnitOfMeasureValueType">
		<xs:annotation>
			<xs:documentation>Unit Of Measure Value contains the numeric value of the actual item or instance as specified by the unit of measure code.  For example, if the unit of measure code contains 'Volume', then the unit of measure value would contain the value '16' that represents the volume of a specified item or instance, etc.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2"/>
			<xs:totalDigits value="13"/>
		</xs:restriction>
	</xs:simpleType>
	<!---->
</xs:schema>
