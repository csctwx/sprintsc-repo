	<xs:complexType name="M2HNumberInfoListType">
		<xs:sequence>
			<xs:element name="m2HNumberInfo" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="socCode" type="tns:SocCode"/>
						<xs:element name="socSeqNumber" type="tns:SocSequenceNumber"/>
						<xs:element name="effectiveDate" type="xs:date"/>
						<xs:element name="expirationDate" type="xs:date" minOccurs="0"/>
						<xs:element name="numberList" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="numberInfo" maxOccurs="100">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="number" type="tns:MdnString"/>
												<xs:element name="effectiveDate" type="xs:date"/>
												<xs:element name="expirationDate" type="xs:date" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
