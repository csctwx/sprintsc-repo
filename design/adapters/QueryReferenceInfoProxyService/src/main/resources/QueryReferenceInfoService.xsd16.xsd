<xs:element name="queryReferenceSocList" type="tns:QueryReferenceSocListRequest"/>
	
<xs:element name="queryReferenceSocListResponse" type="tns:QueryReferenceSocListResponse"/>
	
<xs:complexType name="QueryReferenceSocListResponse">
		
<xs:sequence>
			
<xs:element name="socList" type="tns:SocListResponse" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="QueryReferenceSocListRequest">
		
<xs:sequence>
			
<xs:element name="socList" type="tns:SocList"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="AreaInfo">
		
<xs:choice>
			
<xs:element name="subMarketCode" type="tns:SubMarketCode">
				
<xs:annotation>
					
<xs:documentation>A code that uniquely identifies an particular geographic area</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="csa" type="tns:CommunicationServiceAreaId">
				
<xs:annotation>
					
<xs:documentation>Communication Service Area Identifier uniquely identifies an instance of a geographic area (CSA) served by a communication service provider, which can be broken down into regional, metropolitan, district and smaller sections called ''leaves.''</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	
<xs:complexType name="AttributeInfo">
		
<xs:sequence>
			
<xs:element name="attribCode" type="tns:ServiceOrderAttributeValue">
				
<xs:annotation>
					
<xs:documentation>Soc Attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="AttributeList">
		
<xs:sequence>
			
<xs:element name="attributeInfo" type="tns:AttributeInfo" maxOccurs="unbounded">
				
<xs:annotation>
					
<xs:documentation>Attribute Info</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="CorporationInfoType">
		
<xs:sequence>
			
<xs:element name="corpId" type="tns:CorporationIdentifierType">
				
<xs:annotation>
					
<xs:documentation>Corporation Id</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="accountType" type="tns:BillingAccountTypeCodeType">
				
<xs:annotation>
					
<xs:documentation>Account Type Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="accountSubType" type="tns:BillingAccountSubTypeCodeType">
				
<xs:annotation>
					
<xs:documentation>Account Subtype Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="networkInd" type="tns:NetworkTypeCodeType">
				
<xs:annotation>
					
<xs:documentation>Network Indicator
Values:
C – CDMA
I  -  iDEN   ( I is also used for PowerSource or dual mode phones)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="EtfInformation">
		
<xs:sequence>
			
<xs:element name="fetchEtfInfo" type="tns:FetchInformationFlag">
				
<xs:annotation>
					
<xs:documentation>Flag to indicate a request to fetch ETF Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="contractStartDate" type="tns:ContractStartDate" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Date from which the contract is effective</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="areaInfo" type="tns:AreaInfo" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Choice of csa or subMarket to further identify a particular geographic area</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="corporationInfo" type="tns:CorporationInfoType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Additional fields for fetching EtfInfo when a Corporate early termination fee may exist. </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="itemId" type="tns:ModelNumberType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>If sent in the input, ETF based on itemId will be sent in the response</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="contractMonths" type="tns:CommitmentPeriodMonthQuantityType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>If sent in the input, contractEndDate and contractTotalMonths will be calculated using this value </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="EtfInformationResponse">
		
<xs:sequence>
			
<xs:element name="contractStartDate" type="tns:ContractStartDate" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Contract Effective Date for the input PP/SOC </xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="contractEndDate" type="tns:ContractEndDate" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Contract End Date for the input PP/SOC
Or based on itemId in the input</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="contractTotalMonths" type="tns:CommitmentPeriodMonthQuantity" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Total number of months for the input PP/SOC 
Or based on itemId in the input</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="etfPolicyEffectiveDate" type="tns:EffectiveFromDate" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Date in which the Pro-Rated ETF policy is effective for the inputPP/ SOC.
Device Tier effective Date if etf based on itemId in the input</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="etfMethod" type="tns:EarlyTerminationFeeMethodCode" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Method used for calculating the early termination fee.
Values:
F (flat)
P (pro-rated)
C (contractual)</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="etfAmount" type="tns:EarlyContractTerminationFee" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>ETF Amount
This value is the Monthly ETF Fee for early termination if pro-rated.  The Monthly ETF Fee is defined for contracts that were committed after the pro-rated ETF policy effective date based on the input PP/SOC Or based on itemId sent in the request  If etfMethod is F – Flat or C – Contractual, this value is the ETF Amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="etfMaxAmount" type="tns:EarlyContractTerminationFee" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The maximum early termination fee that a subscriber can pay due to an earlier commitment termination based on the input PP/SOC or based on itemId sent in the request.
Will not be returned if etfMethod = F or C</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="etfMinAmount" type="tns:EarlyContractTerminationFee" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>The minimum early termination fee that a subscriber can pay due to an earlier commitment termination based on the input PP/SOC or based on itemId sent in the request.
Will not be returned if etfMethod = F or C</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="deviceTierCode" type="tns:DeviceEarlyTerminationFeeTierCodeType" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>Tier code from ETF_TIER_DEFINITION for the input itemId</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="deviceTierDesc" type="tns:DeviceEarlyTerminationFeeTierDescriptionType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="FeatureInfo">
		
<xs:sequence>
			
<xs:element name="featureCode" type="tns:FeatureCode">
				
<xs:annotation>
					
<xs:documentation>Feature Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="featureDesc" type="tns:FeatureDescription">
				
<xs:annotation>
					
<xs:documentation>Feature Description.</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="spanishFeatureDesc" type="tns:FeatureDescription" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>FEATURE.SPANISH_FTR_DESC</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="oneTimeChargeInd" type="tns:NonRecurringChargeFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>show whether this is AC/UC/OC/RC type of feature</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="additionalChargeInd" type="tns:AdditionalChargeFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>show whether this is AC/UC/OC/RC type of feature</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="usageChargeInd" type="tns:UsageChargeFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>show whether this is AC/UC/OC/RC type of feature</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="recurringChargeInd" type="tns:MonthlyRecurringChargeFlag" minOccurs="0">
				
<xs:annotation>
					
<xs:documentation>show whether this is AC/UC/OC/RC type of feature</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="FeatureList">
		
<xs:sequence>
			
<xs:element name="featureInfo" type="tns:FeatureInfo" maxOccurs="unbounded">
				
<xs:annotation>
					
<xs:documentation>Feature Info</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="LtsRcInfo">
		
