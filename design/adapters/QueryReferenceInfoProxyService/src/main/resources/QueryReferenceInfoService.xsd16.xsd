<xs:annotation>
					
<xs:documentation>SOC.LTS_MAX_SUB</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="ltsRecurringChargeInfo" type="tns:LtsRcInfo" maxOccurs="unbounded">
				
<xs:annotation>
					
<xs:documentation>LTS Recurring Charge Info</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="MultiTierList">
		
