				</xs:annotation>
			</xs:element>
			
<xs:element name="toRank" type="tns:RankSequenceNumber">
				
<xs:annotation>
					
<xs:documentation>SOC_RC_RANK.TO</xs:documentation>
				</xs:annotation>
			</xs:element>
			
<xs:element name="recurringCharge" type="tns:MonthlyRecurringCharge">
				
<xs:annotation>
					
<xs:documentation>SOC_RC_RANK.RC</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
<xs:complexType name="LtsRcList">
		
<xs:sequence>
			
