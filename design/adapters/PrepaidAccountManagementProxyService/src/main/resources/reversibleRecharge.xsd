		</xsd:annotation>
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="RemarkText">
		<xsd:annotation>
			<xsd:documentation>Remark Text is a NOTE containing textual comment.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="NoteText">
			<xsd:maxLength value="250"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SalesChannelId">
		<xsd:annotation>
			<xsd:documentation>Sales Channel Identifier uniquely identifies a PARTY, internal to Sprint or external, that sells Sprint equipment and services.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="PartyID">
