	<xsd:simpleType name="SubscriberNumber">
		<xsd:annotation>
			<xsd:documentation>Subscriber Number</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="PartyOtherIdentifierID">
