	<xsd:simpleType name="RechargeMethodCode">
		<xsd:annotation>
			<xsd:documentation>Recharge Method Code identifies the method used for the Recharge. Valid values are:

VOT
VRA
VASP
VARC
VMRC
VLBE
PCRC</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="TypeCode">
			<xsd:maxLength value="4"/>
			<!-- xsd:enumeration value="VOT"/>
			<xsd:enumeration value="VRA"/>
			<xsd:enumeration value="VASP"/>
			<xsd:enumeration value="VARC"/>
			<xsd:enumeration value="VMRC"/>
			<xsd:enumeration value="VLBR"/>
			<xsd:enumeration value="PCRC"/ -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RechargeMethodIdentifier">
		<xsd:annotation>
			<xsd:documentation>Recharge Method Identifer identifies the Recharge Method.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="IdentifierString">
			<xsd:maxLength value="45"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RechargeMethodType">
		<xsd:annotation>
			<xsd:documentation>Recharge Method Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="TypeCode"/>
	</xsd:simpleType>
	<xsd:simpleType name="RechargeMethodValues">
		<xsd:annotation>
			<xsd:documentation>Recharge Method Values</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="400"/>
		</xsd:restriction>
