	</xsd:simpleType>
	<xsd:simpleType name="StatusCode">
		<xsd:annotation>
			<xsd:documentation>Status Code</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="SubscriptionIdentifier">
		<xsd:annotation>
			<xsd:documentation>Subscription Identifier uniquely identifies a specific instance of a SUBSCRIBER.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
