	<xsd:simpleType name="ReleaseRechargeIdentifier">
		<xsd:annotation>
			<xsd:documentation>Release Recharge Identifer uniquely identifes an instance of a Release Recharge.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="IdentifierInteger"/>
	</xsd:simpleType>
	<xsd:simpleType name="ReleaseRechargeType">
		<xsd:annotation>
			<xsd:documentation>Release Recharge Type identifies the type of release recharge. Valid values are:

'N' - None
'M' - Main Recharge
'P' - Part of Recharge
</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="TypeCode">
			<xsd:length value="1"/>
			<!-- xsd:enumeration value="N"/>
			<xsd:enumeration value="M"/>
			<xsd:enumeration value="P"/ -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RetailStoreIdentifier">
		<xsd:annotation>
			<xsd:documentation>Retail Store Identifer is the store ID passed from the retailer to inComm.  These are 4 to 6 characters in length sometimes padded with zeros.  Leading 0s should not be stripped off.

(Examples: 001859, 1165, 0649)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SequenceNumber">
		<xsd:annotation>
			<xsd:documentation>Sequence Number identifies the numeric value that is given to a file or record as it is sequentially created. The first file or record will start with 0001 and continue incrementing until a specified maximum value is reached (if a maximum is defined). One use of this element is to assist in identifying missing files.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="SubIdValues">
		<xsd:annotation>
			<xsd:documentation>Sub Id Values</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="80"/>
		</xsd:restriction>
