	<xsd:simpleType name="RechargeStatusCode">
		<xsd:annotation>
			<xsd:documentation>Recharge Status Code identifies the current state of the recharge. Valid values are:

'V' - Valid
'C' - Cancelled
'S' - Suspend
'R' - Restored
'I' - Internal
'P' - Pending</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="StatusCode">
			<xsd:length value="1"/>
			<!-- xsd:enumeration value="V"/>
			<xsd:enumeration value="C"/>
			<xsd:enumeration value="S"/>
			<xsd:enumeration value="R"/>
			<xsd:enumeration value="I"/>
			<xsd:enumeration value="P"/ -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RechargeTypeCode">
		<xsd:annotation>
			<xsd:documentation>Recharge Type Code identifies the type of Recharge.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="1"/>
			<!-- xsd:enumeration value="R"/>
			<xsd:enumeration value="S"/ -->
		</xsd:restriction>
